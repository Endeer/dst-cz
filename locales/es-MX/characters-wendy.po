msgstr ""

#. CHARACTERS.WENDY.DESCRIBE.POCKETWATCH_PORTAL_ENTRANCE.DIFFERENTSHARD
msgctxt "CHARACTERS.WENDY.DESCRIBE.POCKETWATCH_PORTAL_ENTRANCE.DIFFERENTSHARD"
