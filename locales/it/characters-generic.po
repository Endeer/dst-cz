msgstr "Vuole qualcosa da me."

#. CHARACTERS.GENERIC.DESCRIBE.ANTLION.UNHAPPY
msgctxt "CHARACTERS.GENERIC.DESCRIBE.ANTLION.UNHAPPY"
msgid "It looks mad."
msgstr "Sembra arrabbiata."

#. CHARACTERS.GENERIC.DESCRIBE.ANTLION.VERYHAPPY
msgctxt "CHARACTERS.GENERIC.DESCRIBE.ANTLION.VERYHAPPY"
