msgstr "Non ci serve sapere altro."

#. CHARACTERS.WEBBER.ACTIONFAIL.PLANTREGISTRY_RESEARCH_FAIL.GENERIC
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.PLANTREGISTRY_RESEARCH_FAIL.GENERIC"
msgid "We already know everything about this plant!"
msgstr "Sappiamo già tutto su questa pianta!"

#. CHARACTERS.WEBBER.ACTIONFAIL.POUR_WATER.OUT_OF_WATER
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.POUR_WATER.OUT_OF_WATER"
msgid "Oops, out of water!"
msgstr "Ops, siamo a secco!"

#. CHARACTERS.WEBBER.ACTIONFAIL.POUR_WATER_GROUNDTILE.OUT_OF_WATER
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.POUR_WATER_GROUNDTILE.OUT_OF_WATER"
msgid "Aw, we're all out of water."
msgstr "Uffa, siamo proprio a secco."

#. CHARACTERS.WEBBER.ACTIONFAIL.REMOVEMODULES.NO_MODULES
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.REMOVEMODULES.NO_MODULES"
msgid "only_used_by_wx78"
msgstr "only_used_by_wx78"

#. CHARACTERS.WEBBER.ACTIONFAIL.REPAIR.WRONGPIECE
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.REPAIR.WRONGPIECE"
msgid "This two piece puzzle sure is hard!"
msgstr "Questo puzzle di due pezzi è davvero difficile."

#. CHARACTERS.WEBBER.ACTIONFAIL.REPLATE.MISMATCH
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.REPLATE.MISMATCH"
msgid "Hmmm... I think we need a different dish for this."
msgstr "Uhmmm… credo ci voglia un piatto diverso."

#. CHARACTERS.WEBBER.ACTIONFAIL.REPLATE.SAMEDISH
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.REPLATE.SAMEDISH"
msgid "We already put this on a dish."
msgstr "Lo abbiamo già messo su un piatto."

#. CHARACTERS.WEBBER.ACTIONFAIL.ROW_FAIL.BAD_TIMING0
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.ROW_FAIL.BAD_TIMING0"
msgid "We need to paddle at the right time!"
msgstr "Dobbiamo remare al momento giusto!"

#. CHARACTERS.WEBBER.ACTIONFAIL.ROW_FAIL.BAD_TIMING1
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.ROW_FAIL.BAD_TIMING1"
msgid "Whoops! One more time!"
msgstr "Ops! Riproviamo!"

#. CHARACTERS.WEBBER.ACTIONFAIL.ROW_FAIL.BAD_TIMING2
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.ROW_FAIL.BAD_TIMING2"
msgid "Row row row your boat... maybe a bit better next time."
msgstr "Rema, rema la tua barca! Magari la prossima volta andrà meglio."

#. CHARACTERS.WEBBER.ACTIONFAIL.RUMMAGE.GENERIC
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.RUMMAGE.GENERIC"
msgid "That's off-limits."
msgstr "È vietato."

#. CHARACTERS.WEBBER.ACTIONFAIL.RUMMAGE.INUSE
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.RUMMAGE.INUSE"
msgid "It's okay, we can wait for you to finish!"
msgstr "Va bene, possiamo aspettare che finisci!"

#. CHARACTERS.WEBBER.ACTIONFAIL.RUMMAGE.NOTMASTERCHEF
msgctxt "CHARACTERS.WEBBER.ACTIONFAIL.RUMMAGE.NOTMASTERCHEF"
