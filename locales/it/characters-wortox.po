msgid "Sleeping like the fishes. Hyuyu!"
msgstr "Dormire come i pesci. Hiuuù!"

#. CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE
msgctxt "CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE"
msgid "A house that's cobbled from bits and bobs."
msgstr "Una casa fabbricata con tante cianfrusaglie."

#. CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE_RUBBLE
msgctxt "CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE_RUBBLE"
msgid "Nothing but bits and bobs left."
msgstr "Sono rimaste solo le cianfrusaglie."

#. CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_SYRUP
msgctxt "CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_SYRUP"
msgid "For making sweet treats."
msgstr "Per fare dolci delizie."

#. CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_TOMATO
msgctxt "CHARACTERS.WORTOX.DESCRIBE.QUAGMIRE_TOMATO"
