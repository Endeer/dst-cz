msgstr "Nie przestrzegasz reguł, których nauczył mnie dziadek."

#. CHARACTERS.WEBBER.DESCRIBE.BISHOP_CHARGE_HIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BISHOP_CHARGE_HIT"
msgid "Owie!"
msgstr "Auć!"

#. CHARACTERS.WEBBER.DESCRIBE.BISHOP_NIGHTMARE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BISHOP_NIGHTMARE"
msgid "Bishop of bad dreams."
msgstr "Goniec z koszmarów."

#. CHARACTERS.WEBBER.DESCRIBE.BLOWDART_FIRE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLOWDART_FIRE"
msgid "One step above blowing hot air."
msgstr "Trochę jak chuchanie, tylko bardziej."

#. CHARACTERS.WEBBER.DESCRIBE.BLOWDART_LAVA
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLOWDART_LAVA"
msgid "We like fighting from far away."
msgstr "Lubimy walkę na odległość."

#. CHARACTERS.WEBBER.DESCRIBE.BLOWDART_LAVA2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLOWDART_LAVA2"
msgid "Those look like fun!"
msgstr "Ale frajda!"

#. CHARACTERS.WEBBER.DESCRIBE.BLOWDART_PIPE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLOWDART_PIPE"
msgid "Same as blowing bubbles."
msgstr "To jak dmuchanie baniek."

#. CHARACTERS.WEBBER.DESCRIBE.BLOWDART_SLEEP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLOWDART_SLEEP"
msgid "Airborne sleeping agent."
msgstr "Powietrzny środek nasenny."

#. CHARACTERS.WEBBER.DESCRIBE.BLOWDART_YELLOW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLOWDART_YELLOW"
msgid "It's a shock to the system."
msgstr "Jedna z moich ulubionych zabawek."

#. CHARACTERS.WEBBER.DESCRIBE.BLUEAMULET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUEAMULET"
msgid "I guess it's nice."
msgstr "Chyba całkiem fajny."

#. CHARACTERS.WEBBER.DESCRIBE.BLUEGEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUEGEM"
msgid "Glittering and cool."
msgstr "Ale się ładne błyszczy."

#. CHARACTERS.WEBBER.DESCRIBE.BLUEMOONEYE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUEMOONEYE"
msgid "Hey! Did anyone lose an eye?"
msgstr "Ktoś zgubił oko?"

#. CHARACTERS.WEBBER.DESCRIBE.BLUEPRINT.COMMON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUEPRINT.COMMON"
msgid "\"Follow the instructions\", mum always said."
msgstr "Mama zawsze mówiła, żeby najpierw przeczytać instrukcję."

#. CHARACTERS.WEBBER.DESCRIBE.BLUEPRINT.RARE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUEPRINT.RARE"
msgid "This one feels special."
msgstr "Ten wydaje się jakiś wyjątkowy."

#. CHARACTERS.WEBBER.DESCRIBE.BLUE_CAP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUE_CAP"
msgid "You'd have to be crazy..."
msgstr "Jestem mały, ale nie głupi..."

#. CHARACTERS.WEBBER.DESCRIBE.BLUE_CAP_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUE_CAP_COOKED"
msgid "Good thing we're feeling healthy."
msgstr "Dobrze, że nic nam nie dolega."

#. CHARACTERS.WEBBER.DESCRIBE.BLUE_MUSHROOM.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUE_MUSHROOM.GENERIC"
msgid "Vroom vroom, mushroom."
msgstr "Kiedyś lubiłem jeździć na grzyby."

#. CHARACTERS.WEBBER.DESCRIBE.BLUE_MUSHROOM.INGROUND
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUE_MUSHROOM.INGROUND"
msgid "Hiding, are we?"
msgstr "Bawimy się w chowanego?"

#. CHARACTERS.WEBBER.DESCRIBE.BLUE_MUSHROOM.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BLUE_MUSHROOM.PICKED"
msgid "Maybe it will regrow."
msgstr "Może odrośnie."

#. CHARACTERS.WEBBER.DESCRIBE.BOARDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOARDS"
msgid "Logs, but flat."
msgstr "Kłoda, tylko płaska."

#. CHARACTERS.WEBBER.DESCRIBE.BOARON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOARON"
msgid "Leave us alone!"
msgstr "Zostaw nas w spokoju!"

#. CHARACTERS.WEBBER.DESCRIBE.BOARRIOR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOARRIOR"
msgid "You don't look so tough!"
msgstr "Nie wyglądasz wcale na twardziela!"

#. CHARACTERS.WEBBER.DESCRIBE.BOATFRAGMENT03
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOATFRAGMENT03"
msgid "Oh no, someone had an accident!"
msgstr "Och, nie, ktoś miał wypadek!"

#. CHARACTERS.WEBBER.DESCRIBE.BOATFRAGMENT04
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOATFRAGMENT04"
msgid "Oh no, someone had an accident!"
msgstr "Och, nie, ktoś miał wypadek!"

#. CHARACTERS.WEBBER.DESCRIBE.BOATFRAGMENT05
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOATFRAGMENT05"
msgid "Oh no, someone had an accident!"
msgstr "Och, nie, ktoś miał wypadek!"

#. CHARACTERS.WEBBER.DESCRIBE.BOATPATCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOATPATCH"
msgid "It makes the boat feel better."
msgstr "Sprawia, że łódka czuje się lepiej."

#. CHARACTERS.WEBBER.DESCRIBE.BOAT_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOAT_ITEM"
msgid "That's for building a boat."
msgstr "Z tego będzie łódka."

#. CHARACTERS.WEBBER.DESCRIBE.BOAT_LEAK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOAT_LEAK"
msgid "Quick, plug it up!"
msgstr "Szybko, trzeba to zatkać!"

#. CHARACTERS.WEBBER.DESCRIBE.BONESHARD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BONESHARD"
msgid "Boney bits."
msgstr "Kosteczki."

#. CHARACTERS.WEBBER.DESCRIBE.BONESOUP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BONESOUP"
msgid "Warly makes really good stuff."
msgstr "Warly naprawdę dobrze gotuje."

#. CHARACTERS.WEBBER.DESCRIBE.BONESTEW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BONESTEW"
msgid "Smells like Sunday supper."
msgstr "Pachnie jak niedzielny obiad."

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_BIRDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_BIRDS"
msgid "This one has pictures!"
msgstr "O, a w tej są obrazki!"

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_BRIMSTONE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_BRIMSTONE"
msgid "We don't like how that one ends!"
msgstr "Nie podoba się nam zakończenie!"

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_ELEMENTAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_ELEMENTAL"
msgid "Are those even words in there?"
msgstr "Czy to są w ogóle jakieś słowa?"

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_FOSSIL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_FOSSIL"
msgid "We can read the words but we can't make them work."
msgstr "Umiemy przeczytać te słowa, ale one chyba nie działają."

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_GARDENING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_GARDENING"
msgid "Why should we read about flowers when we can pick them?"
msgstr "Po co czytać o kwiatkach, skoro możemy je po prostu zbierać?"

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_HORTICULTURE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_HORTICULTURE"
msgid "Reading about veggies is almost as bad as eating them!"
msgstr "Czytanie o warzywach jest prawie tak straszne jak ich jedzenie!"

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_SILVICULTURE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_SILVICULTURE"
msgid "What else is there to know about trees?"
msgstr "Czego jeszcze można się dowiedzieć o drzewach?"

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_SLEEP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_SLEEP"
msgid "It's... beddy-bye time..."
msgstr "Czas na spanie... Dobranoc..."

#. CHARACTERS.WEBBER.DESCRIBE.BOOK_TENTACLES
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOK_TENTACLES"
msgid "Why are the pages all slimy?"
msgstr "Dlaczego wszystkie kartki są takie obślizgłe?"

#. CHARACTERS.WEBBER.DESCRIBE.BOOMERANG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BOOMERANG"
msgid "Boomerangarangarang!"
msgstr "Buuuuuuuuuuumerang!"

#. CHARACTERS.WEBBER.DESCRIBE.BRUSH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BRUSH"
msgid "It's more scratchy than anything."
msgstr "Drapiące."

#. CHARACTERS.WEBBER.DESCRIBE.BUGNET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUGNET"
msgid "I'm not a bug! We're an arachnid!"
msgstr "Nie jestem robakiem! Jestem pajęczakiem!"

#. CHARACTERS.WEBBER.DESCRIBE.BULLKELP_PLANT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BULLKELP_PLANT.GENERIC"
msgid "There's some kelp floating in the water!"
msgstr "W wodzie są jakieś wodorosty!"

#. CHARACTERS.WEBBER.DESCRIBE.BULLKELP_PLANT.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BULLKELP_PLANT.PICKED"
msgid "We gave it a haircut."
msgstr "Trochę je przycięliśmy."

#. CHARACTERS.WEBBER.DESCRIBE.BULLKELP_ROOT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BULLKELP_ROOT"
msgid "It would be happier if we planted it."
msgstr "Będzie szczęśliwsze, jeśli je zasadzimy."

#. CHARACTERS.WEBBER.DESCRIBE.BUNDLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUNDLE"
msgid "That's one of my favorite things!"
msgstr "Jedna z moich ulubionych rzeczy!"

#. CHARACTERS.WEBBER.DESCRIBE.BUNDLEWRAP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUNDLEWRAP"
msgid "Let's cocoon something for later!"
msgstr "Zapakujmy coś w kokon na później!"

#. CHARACTERS.WEBBER.DESCRIBE.BUNNYMAN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUNNYMAN"
msgid "Hop along, lil bunny."
msgstr "Hop, hop, króliczku."

#. CHARACTERS.WEBBER.DESCRIBE.BUNNYSTEW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUNNYSTEW"
msgid "We were told we can't lick it clean..."
msgstr "Nie pozwolono nam zjeść wszystkiego..."

#. CHARACTERS.WEBBER.DESCRIBE.BURNT_MARSH_BUSH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BURNT_MARSH_BUSH"
msgid "All burned up."
msgstr "Wszystko poszło z dymem."

#. CHARACTERS.WEBBER.DESCRIBE.BUSHHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUSHHAT"
msgid "For looking bushier!"
msgstr "Można się łatwiej wtopić w tło."

#. CHARACTERS.WEBBER.DESCRIBE.BUTTER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUTTER"
msgid "Butter is better."
msgstr "Pyszna rzecz."

#. CHARACTERS.WEBBER.DESCRIBE.BUTTERFLY.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUTTERFLY.GENERIC"
msgid "Look at it, flitting around happily."
msgstr "Patrz jak fruwa, latawiec jeden, motyl wyzwolony."

#. CHARACTERS.WEBBER.DESCRIBE.BUTTERFLY.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUTTERFLY.HELD"
msgid "Not so happy now, are we?"
msgstr "Koniec fruwania."

#. CHARACTERS.WEBBER.DESCRIBE.BUTTERFLYMUFFIN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUTTERFLYMUFFIN"
msgid "Do you know the muffin spider?"
msgstr "Słyszałeś o babeczkowym pająku?"

#. CHARACTERS.WEBBER.DESCRIBE.BUTTERFLYWINGS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUTTERFLYWINGS"
msgid "Pick our teeth with butterfly bones."
msgstr "Możemy podłubać sobie w zębach kośćmi motyla."

#. CHARACTERS.WEBBER.DESCRIBE.BUZZARD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.BUZZARD"
msgid "You won't find any carrion here. We're stuck like this."
msgstr "Nie znajdziesz tu żadnej padliny. Tak sobie wspólnie żyjemy."

#. CHARACTERS.WEBBER.DESCRIBE.CABBAGEROLLS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CABBAGEROLLS"
msgid "We hope Ms. Wickerbottom doesn't find out we didn't eat the cabbage..."
msgstr "Mam nadzieję, że pani Wiklińska nie dowie się, że nie zjedliśmy kapusty..."

#. CHARACTERS.WEBBER.DESCRIBE.CACTUS.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CACTUS.GENERIC"
msgid "It's got more things coming out of it than we do."
msgstr "Wystaje z niego więcej rzeczy niż z nas."

#. CHARACTERS.WEBBER.DESCRIBE.CACTUS.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CACTUS.PICKED"
msgid "It's got no meat left in it."
msgstr "Nie ma już żadnego mięsa."

#. CHARACTERS.WEBBER.DESCRIBE.CACTUS_FLOWER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CACTUS_FLOWER"
msgid "This part is nice."
msgstr "Ta część jest całkiem ładna."

#. CHARACTERS.WEBBER.DESCRIBE.CACTUS_MEAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CACTUS_MEAT"
msgid "Haven't had enough, huh?"
msgstr "Mało ci, co?"

#. CHARACTERS.WEBBER.DESCRIBE.CACTUS_MEAT_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CACTUS_MEAT_COOKED"
msgid "Take that, pokey things!"
msgstr "A macie, wredne kolce!"

#. CHARACTERS.WEBBER.DESCRIBE.CALIFORNIAROLL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CALIFORNIAROLL"
msgid "Hmm, something smells fishy about this!"
msgstr "Morsko-owocowy przysmak!"

#. CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.EMBERS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.EMBERS"
msgid "We should put something on the fire before it goes out."
msgstr "Musimy wrzucić coś do ogniska, zanim zgaśnie."

#. CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.GENERIC"
msgid "Sure beats the cold. And darkness."
msgstr "Doskonały sposób na zimno. I ciemność."

#. CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.HIGH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.HIGH"
msgid "That fire is huge!"
msgstr "Ale wielki ogień!"

#. CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.LOW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.LOW"
msgid "The fire's getting a touch low."
msgstr "Ogień robi się malutki."

#. CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.NORMAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.NORMAL"
msgid "Nice and warm."
msgstr "Ciepło i przyjemnie."

#. CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.OUT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAMPFIRE.OUT"
msgid "Well, that's done."
msgstr "Skończyło się."

#. CHARACTERS.WEBBER.DESCRIBE.CANARY.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CANARY.GENERIC"
msgid "That's a yellow bird."
msgstr "Żółty ptaszek."

#. CHARACTERS.WEBBER.DESCRIBE.CANARY.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CANARY.HELD"
msgid "Caught you!"
msgstr "Mamy cię!"

#. CHARACTERS.WEBBER.DESCRIBE.CANARY_POISONED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CANARY_POISONED"
msgid "Uh, are you feeling okay?"
msgstr "Dobrze się czujesz?"

#. CHARACTERS.WEBBER.DESCRIBE.CANDYBAG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CANDYBAG"
msgid "Treats, treats, treats!"
msgstr "Cukierki!"

#. CHARACTERS.WEBBER.DESCRIBE.CANE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CANE"
msgid "One more point of contact couldn't slow us down."
msgstr "Jedna dodatkowa kończyna na pewno nas nie spowolni."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALCANNON.COOLDOWN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALCANNON.COOLDOWN"
msgid "Yay!!"
msgstr "Jupi!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALCANNON.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALCANNON.GENERIC"
msgid "Can we turn it on? Pretty please?"
msgstr "Możemy to włączyć? Bardzo prosimy!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALCANNON_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALCANNON_KIT"
msgid "We can't wait to play with it!"
msgstr "Nie możemy się doczekać, żeby się tym pobawić!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_EGGRIDE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_EGGRIDE"
msgid "We wish it was a little bigger so we could ride too!"
msgstr "Chcielibyśmy, żeby było trochę większe. Też byśmy sobie pojeździli!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_EGGRIDE_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_EGGRIDE_KIT"
msgid "We can put it together!"
msgstr "Damy radę to złożyć!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE.GENERIC"
msgid "It's a little wooden statue."
msgstr "Mały drewniany posążek."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE.RARE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE.RARE"
msgid "Oooh, we got an extra special one!"
msgstr "Ooo, mamy coś niezwykle niezwykłego!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE.UNCOMMON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE.UNCOMMON"
msgid "We're going to collect all of them!"
msgstr "Zbierzemy je wszystkie!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_FIGURE_KIT"
msgid "We can't wait to see what's inside!"
msgstr "Tak bardzo chcemy zobaczyć, co jest w środku!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_LAMP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_LAMP"
msgid "Night lights make the dark a little less scary."
msgstr "Światło sprawia, że ciemność jest trochę mniej straszna."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_LAMP_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_LAMP_KIT"
msgid "We can put it together!"
msgstr "Damy radę to złożyć!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_PLANT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_PLANT"
msgid "This tree is so small!"
msgstr "To drzewo jest takie malutkie!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_PLANT_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALDECOR_PLANT_KIT"
msgid "We can put it together!"
msgstr "Damy radę to złożyć!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALFOOD_CORNTEA
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALFOOD_CORNTEA"
msgid "Is Ms. Wickerbottom trying to trick us into eating our vegetables again?"
msgstr "Czy pani Wiklińska znowu próbuje podstępem nakłonić nas do jedzenia warzyw?"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_FOOD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_FOOD"
msgid "We think Wurt might eat these too."
msgstr "Wurt też chyba może je zjeść."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_KIT"
msgid "We can help the birds set up."
msgstr "Pomożemy tym ptaszkom."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_NEST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_NEST"
msgid "It's a little bird door."
msgstr "Drzwiczki dla ptaszków."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_STATION.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_STATION.GENERIC"
msgid "It'll let us play if we give it something shiny."
msgstr "Pozwoli nam zagrać w zamian za coś błyszczącego."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_STATION.PLAYING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_FEEDCHICKS_STATION.PLAYING"
msgid "Don't worry bird mom, all your babies will get fed."
msgstr "Nie martw się, ptasia mamo, nakarmimy wszystkie twoje ptaszki."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_CHICK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_CHICK"
msgid "Come back, eggy!"
msgstr "Wracaj do nas!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_KIT"
msgid "We can help the birds set up."
msgstr "Pomożemy tym ptaszkom."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_STATION.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_STATION.GENERIC"
msgid "It'll let us play if we give it something shiny."
msgstr "Pozwoli nam zagrać w zamian za coś błyszczącego."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_STATION.PLAYING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_HERDING_STATION.PLAYING"
msgid "We're really good at herding things!"
msgstr "Potrafimy zaganiać różne rzeczy!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_CARD.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_CARD.GENERIC"
msgid "It's a little bird door."
msgstr "Drzwiczki dla ptaszków."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_CARD.PLAYING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_CARD.PLAYING"
msgid "We're pretty sure it's this one! Or maybe that one."
msgstr "Jesteśmy pewni, że to ta! Albo ta."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_KIT"
msgid "We always wondered what it would be like to run away and join a carnival."
msgstr "Zawsze nas zastanawiało, jakby to było, gdybyśmy uciekli i dołączyli do wesołego miasteczka."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_STATION.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_STATION.GENERIC"
msgid "It'll let us play if we give it something shiny."
msgstr "Pozwoli nam zagrać w zamian za coś błyszczącego."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_STATION.PLAYING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVALGAME_MEMORY_STATION.PLAYING"
msgid "It's that one over there! Or... was it the other one..."
msgstr "To to! Albo... tamto..."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_BALL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_BALL"
msgid "We wanna play with it!"
msgstr "Chcemy się tym pobawić!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_CROWKID
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_CROWKID"
msgid "Are you a boy who was eaten by a crow?"
msgstr "Jesteś chłopcem, którego zjadła wrona?"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_GAMETOKEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_GAMETOKEN"
msgid "Wow, it's so shiny!"
msgstr "Rety, ale się błyszczy!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_HOST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_HOST"
msgid "That's the biggest crow we've ever seen!"
msgstr "W życiu nie widzieliśmy większej wrony!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA.GENERIC"
msgid "It would look better with more decorations around."
msgstr "Byłoby lepiej, gdyby było tu więcej dekoracji."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA.LEVEL_2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA.LEVEL_2"
msgid "We're pretty sure we can fit even more decorations around it!"
msgstr "Na pewno zmieścimy tutaj więcej dekoracji!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA.LEVEL_3
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA.LEVEL_3"
msgid "It's perfect!"
msgstr "Idealnie!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PLAZA_KIT"
msgid "We need to find a good planting spot."
msgstr "Musimy znaleźć dobre miejsce."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZEBOOTH.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZEBOOTH.GENERIC"
msgid "We're saving up our tickets for that one in the back!"
msgstr "Oszczędzamy losy na tę nagrodę z tyłu!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZEBOOTH_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZEBOOTH_KIT"
msgid "We can't wait to see what kind of prizes they have!"
msgstr "Nie możemy się doczekać, żeby zobaczyć te nagrody!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZETICKET.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZETICKET.GENERIC"
msgid "We won a ticket!"
msgstr "Wygraliśmy los!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZETICKET.GENERIC_LARGESTACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZETICKET.GENERIC_LARGESTACK"
msgid "It's a good thing we have extra arms to carry all these tickets!"
msgstr "Dobrze, że mamy dodatkowe ręce do noszenia tych wszystkich losów!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZETICKET.GENERIC_SMALLSTACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_PRIZETICKET.GENERIC_SMALLSTACK"
msgid "Maybe now we can go get a prize!"
msgstr "Teraz możemy już chyba iść po nagrodę!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_SEEDPACKET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_SEEDPACKET"
msgid "We should've gotten the popcorn instead..."
msgstr "Szkoda, że nie dostaliśmy popcornu..."

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_VEST_A
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_VEST_A"
msgid "We can look just like a crow now! Caw Caw!"
msgstr "Teraz będziemy wyglądać jak wrona! Kra, kra!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_VEST_B
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_VEST_B"
msgid "We always wanted a cape!"
msgstr "Zawsze chcieliśmy mieć pelerynę!"

#. CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_VEST_C
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARNIVAL_VEST_C"
msgid "Did they forget to add the bottom half?"
msgstr "Zrobili tylko połowę?"

#. CHARACTERS.WEBBER.DESCRIBE.CARRAT.DEAD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARRAT.DEAD"
msgid "Oh no!"
msgstr "Och, nie!"

#. CHARACTERS.WEBBER.DESCRIBE.CARRAT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARRAT.GENERIC"
msgid "Hey, little friend!"
msgstr "Hej, maluszku!"

#. CHARACTERS.WEBBER.DESCRIBE.CARRAT.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARRAT.HELD"
msgid "We'll keep you safe."
msgstr "Będziemy o ciebie dbać."

#. CHARACTERS.WEBBER.DESCRIBE.CARRAT.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARRAT.SLEEPING"
msgid "Night night."
msgstr "Pora na dobranoc."

#. CHARACTERS.WEBBER.DESCRIBE.CARRAT_GHOSTRACER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARRAT_GHOSTRACER"
msgid "That one looks kind of spooky..."
msgstr "Ta wygląda trochę strasznie..."

#. CHARACTERS.WEBBER.DESCRIBE.CARROT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARROT"
msgid "I sort of miss being forced to eat these."
msgstr "W sumie trochę tęsknie za czasami, gdy zmuszano mnie, żebym je jadł."

#. CHARACTERS.WEBBER.DESCRIBE.CARROT_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARROT_COOKED"
msgid "Easier for us both."
msgstr "Tak będzie łatwiej."

#. CHARACTERS.WEBBER.DESCRIBE.CARROT_OVERSIZED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARROT_OVERSIZED"
msgid "It's so big!"
msgstr "Ale wielka!"

#. CHARACTERS.WEBBER.DESCRIBE.CARROT_PLANTED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARROT_PLANTED"
msgid "Bury your head, carrot."
msgstr "Chowaj głowę, marchewko!"

#. CHARACTERS.WEBBER.DESCRIBE.CARROT_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARROT_SEEDS"
msgid "We could grow something with these."
msgstr "Można by z nich coś wyhodować."

#. CHARACTERS.WEBBER.DESCRIBE.CARTOGRAPHYDESK.BURNING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARTOGRAPHYDESK.BURNING"
msgid "Ohh no, uh, oh no!"
msgstr "Oj, oj, ojoj!"

#. CHARACTERS.WEBBER.DESCRIBE.CARTOGRAPHYDESK.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARTOGRAPHYDESK.BURNT"
msgid "That's probably why I wasn't allowed in father's study."
msgstr "Pewnie dlatego tata nie pozwalał mi wchodzić do swojego biura."

#. CHARACTERS.WEBBER.DESCRIBE.CARTOGRAPHYDESK.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CARTOGRAPHYDESK.GENERIC"
msgid "Heh heh. I was never allowed in father's study."
msgstr "He, he. Tata nigdy nie pozwalał mi wchodzić do swojego biura."

#. CHARACTERS.WEBBER.DESCRIBE.CATCOON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CATCOON"
msgid "A bit more feral than grandpa's cat."
msgstr "Trochę dzikszy niż kot dziadziunia."

#. CHARACTERS.WEBBER.DESCRIBE.CATCOONDEN.EMPTY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CATCOONDEN.EMPTY"
msgid "The nine lives thing is true!"
msgstr "To o dziewięciu życiach, to prawda!"

#. CHARACTERS.WEBBER.DESCRIBE.CATCOONDEN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CATCOONDEN.GENERIC"
msgid "Trunk house."
msgstr "Drewniany domek."

#. CHARACTERS.WEBBER.DESCRIBE.CATCOONHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CATCOONHAT"
msgid "Hat of a cat."
msgstr "Czapkot."

#. CHARACTERS.WEBBER.DESCRIBE.CATTOY_MOUSE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CATTOY_MOUSE"
msgid "Run little mouse, run!"
msgstr "Uciekaj, mała myszko, uciekaj!"

#. CHARACTERS.WEBBER.DESCRIBE.CAVEIN_BOULDER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVEIN_BOULDER.GENERIC"
msgid "We'll need to mine it down, I guess."
msgstr "Trzeba będzie wydobyć zeń surowce."

#. CHARACTERS.WEBBER.DESCRIBE.CAVEIN_BOULDER.RAISED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVEIN_BOULDER.RAISED"
msgid "Gotta get rid of the other boulders first."
msgstr "Wpierw musimy pozbyć się innych."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA"
msgid "A bundle of bananas."
msgstr "Kiść bananów."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA_BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA_BURNT"
msgid "Oopsie doodle."
msgstr "Ojejciu."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA_COOKED"
msgid "Hot mush."
msgstr "Gorąca papka."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA_TREE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_BANANA_TREE"
msgid "It's a tropical cave tree, of course."
msgstr "Tropikalne jaskiniowe drzewo. Normalka."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE"
msgid "It's plugged up."
msgstr "Zamknięte."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_OPEN.FULL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_OPEN.FULL"
msgid "We'll explore when everyone else has had their turn!"
msgstr "Zabierzemy się do pracy, gdy inni już skończą!"

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_OPEN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_OPEN.GENERIC"
msgid "We'd rather visit our spider friends on the surface."
msgstr "Wolelibyśmy odwiedzić naszych pajęczych przyjaciół na powierzchni."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_OPEN.OPEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_OPEN.OPEN"
msgid "I can hear some spider friends down there."
msgstr "Słyszę na dole naszych pajęczych kolegów."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_RUINS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_ENTRANCE_RUINS"
msgid "It's plugged up."
msgstr "Zamknięte."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_EXIT.FULL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_EXIT.FULL"
msgid "They don't have enough space for us!"
msgstr "Nie ma tam dla nas miejsca!"

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_EXIT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_EXIT.GENERIC"
msgid "We like it down here."
msgstr "Podoba nam się tu."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_EXIT.OPEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_EXIT.OPEN"
msgid "It leads back to the light."
msgstr "Prowadzi z powrotem do światła."

#. CHARACTERS.WEBBER.DESCRIBE.CAVE_FERN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CAVE_FERN"
msgid "Swirly plants."
msgstr "Zakręcone rośliny."

#. CHARACTERS.WEBBER.DESCRIBE.CEVICHE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CEVICHE"
msgid "Yucky!"
msgstr "Fuj!"

#. CHARACTERS.WEBBER.DESCRIBE.CHARCOAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHARCOAL"
msgid "Hard and black, like my better half."
msgstr "Czarny i twardy, jak moja lepsza połowa."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSJUNK1
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSJUNK1"
msgid "A mess of chess."
msgstr "Szachowa masakra."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSJUNK2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSJUNK2"
msgid "A mess of chess."
msgstr "Szachowa masakra."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSJUNK3
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSJUNK3"
msgid "A mess of chess."
msgstr "Szachowa masakra."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ANCHOR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ANCHOR"
msgid "Anchors away!"
msgstr "Podnieść kotwicę!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ANTLION
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ANTLION"
msgid "It's so life-like the mane looks fluffy!"
msgstr "Grzywa jest puszysta jak prawdziwa!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BEARGER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BEARGER"
msgid "The fur looks so real!"
msgstr "To futro jest jak prawdziwe!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BEEFALO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BEEFALO"
msgid "Now we want to go for a beefalo ride!"
msgstr "Teraz chcemy się przejechać na wołku!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BEEQUEEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BEEQUEEN"
msgid "Sweet!"
msgstr "Słodkie!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BISHOP.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BISHOP.GENERIC"
msgid "We've never seen toys so big."
msgstr "Nigdy nie widzieliśmy takich wielkich zabawek."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BISHOP.STRUGGLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BISHOP.STRUGGLE"
msgid "It's... alive?!"
msgstr "Czy on... żyje?!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BUTTERFLY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_BUTTERFLY"
msgid "It looks kinda yummy, doesn't it?"
msgstr "Wygląda smacznie, co nie?"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CARRAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CARRAT"
msgid "It makes us feel content."
msgstr "Sprawia, że czujemy się zadowoleni."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CATCOON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CATCOON"
msgid "We respect the butterfly hunter."
msgstr "Szanujemy łowcę motyli."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CLAYHOUND
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CLAYHOUND"
msgid "Puppy!"
msgstr "Szczeniaczek!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CLAYWARG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CLAYWARG"
msgid "That's a big, bad dog."
msgstr "Aaaale zły pies!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CRABKING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_CRABKING"
msgid "He didn't seem very happy."
msgstr "Nie wyglądał na szczęśliwego."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_DEERCLOPS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_DEERCLOPS"
msgid "This is a really good sculpture."
msgstr "To naprawdę imponująca rzeźba!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_DRAGONFLY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_DRAGONFLY"
msgid "We can practically feel the fire!"
msgstr "To naprawdę ważka kwestia!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_EYEOFTERROR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_EYEOFTERROR"
msgid "We think we could've been friends."
msgstr "Mogliśmy się zaprzyjaźnić."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_FORMAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_FORMAL"
msgid "Reminds me of grandpa."
msgstr "Przypomina mi dziadka."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_GUARDIANPHASE3
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_GUARDIANPHASE3"
msgid "It can't still see us... right?"
msgstr "Nie widzi nas... prawda?"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_HORNUCOPIA
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_HORNUCOPIA"
msgid "Boy, I wish we could eat it."
msgstr "Szkoda, że nie możemy go zjeść."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KITCOON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KITCOON"
msgid "Whoah! I hope they don't fall over..."
msgstr "Rety! Mam nadzieję, że się nie przewrócą..."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KLAUS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KLAUS"
msgid "Can we decorate it?"
msgstr "Możemy go przyozdobić?"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KNIGHT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KNIGHT.GENERIC"
msgid "We could sculpt a stallion battalion!"
msgstr "Tyloma kończynami uwiniemy się w pięć minut."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KNIGHT.STRUGGLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_KNIGHT.STRUGGLE"
msgid "It's... alive?!"
msgstr "Czy on... żyje?!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MALBATROSS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MALBATROSS"
msgid "Not such a big bad bird now!"
msgstr "Teraz nie jest już taki straszny!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MINOTAUR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MINOTAUR"
msgid "We feel a bit bad for stealing their treasure."
msgstr "Trochę nam przykro, że skradziono ci skarb."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MOON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MOON"
msgid "Almost as pretty as the real thing."
msgstr "Prawie tak ładny, jak w rzeczywistości."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MOOSEGOOSE.1
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MOOSEGOOSE.1"
msgid "She doesn't look so mean."
msgstr "Nie wygląda mi zbyt złośliwie."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MUSE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_MUSE"
msgid "We're sure she's nicer than she looks."
msgstr "Na pewno jest sympatyczniejsza, niż się wydaje."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_PAWN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_PAWN"
msgid "Pawns are just as important as the rest of the pieces."
msgstr "Pionki są równie ważne, co figury."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_PIPE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_PIPE"
msgid "Bubbles!"
msgstr "Bańki!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ROOK.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ROOK.GENERIC"
msgid "Where's the king of the castle?"
msgstr "A gdzie król tego zamku?"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ROOK.STRUGGLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_ROOK.STRUGGLE"
msgid "It's... alive?!"
msgstr "Czy on... żyje?!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_STALKER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_STALKER"
msgid "Still kind of spooky."
msgstr "Wciąż trochę upiorne."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_TOADSTOOL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_TOADSTOOL"
msgid "Now we can play leap frog with it!"
msgstr "Możemy pograć w skaczące żabki."

#. CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_TWINSOFTERROR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESSPIECE_TWINSOFTERROR"
msgid "Now they'll always be together."
msgstr "Teraz już zawsze będą razem."

#. CHARACTERS.WEBBER.DESCRIBE.CHESTER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESTER"
msgid "Haha. You make all our stuff slobbery."
msgstr "Ha, ha! Przez ciebie wszystko mamy wilgotne."

#. CHARACTERS.WEBBER.DESCRIBE.CHESTER_EYEBONE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESTER_EYEBONE.GENERIC"
msgid "Peekaboo."
msgstr "A kuku!"

#. CHARACTERS.WEBBER.DESCRIBE.CHESTER_EYEBONE.WAITING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHESTER_EYEBONE.WAITING"
msgid "We spy a tired eye."
msgstr "Chyba się zmęczyła."

#. CHARACTERS.WEBBER.DESCRIBE.CHUM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CHUM"
msgid "We like feeding the fish!"
msgstr "Lubimy karmić rybki!"

#. CHARACTERS.WEBBER.DESCRIBE.CLAYHOUND.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CLAYHOUND.GENERIC"
msgid "Sit! Stay?"
msgstr "Siad! Łapa?"

#. CHARACTERS.WEBBER.DESCRIBE.CLAYHOUND.STATUE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CLAYHOUND.STATUE"
msgid "It looks like a big puppy."
msgstr "Kawał szczeniaczka!"

#. CHARACTERS.WEBBER.DESCRIBE.CLAYWARG.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CLAYWARG.GENERIC"
msgid "N-nice puppy!"
msgstr "D-dobry piesek!"

#. CHARACTERS.WEBBER.DESCRIBE.CLAYWARG.STATUE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CLAYWARG.STATUE"
msgid "It's got no eyes. Creepy!"
msgstr "Nie ma oczu. Trochę straszne!"

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.EMBERS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.EMBERS"
msgid "We should put something on the fire before it goes out."
msgstr "Musimy wrzucić coś do ogniska, zanim zgaśnie."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.GENERIC"
msgid "Sure beats the heat. And darkness."
msgstr "Doskonały sposób na ciepło. I ciemność."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.HIGH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.HIGH"
msgid "That fire is huge!"
msgstr "Ale wielki ogień!"

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.LOW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.LOW"
msgid "The fire's getting a touch low."
msgstr "Ogień robi się malutki."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.NORMAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.NORMAL"
msgid "Nice and cool."
msgstr "Chłodno i przyjemnie."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.OUT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIRE.OUT"
msgid "Well, that's done."
msgstr "Skończyło się."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.EMBERS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.EMBERS"
msgid "We should put something on the fire before it goes out."
msgstr "Musimy wrzucić coś do ogniska, zanim zgaśnie."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.GENERIC"
msgid "Sure beats the heat. And darkness."
msgstr "Doskonały sposób na ciepło. I ciemność."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.HIGH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.HIGH"
msgid "That fire is huge!"
msgstr "Ale wielki ogień!"

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.LOW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.LOW"
msgid "The fire's getting a touch low."
msgstr "Ogień robi się malutki."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.NORMAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.NORMAL"
msgid "Nice and cool."
msgstr "Chłodno i przyjemnie."

#. CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.OUT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COLDFIREPIT.OUT"
msgid "Well, that's done. But not forever!"
msgstr "Skończyło się. Choć nie na zawsze!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.E
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.E"
msgid "East!"
msgstr "Wschód!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.GENERIC"
msgid "No reading!"
msgstr "Brak odczytu!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.N
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.N"
msgid "North!"
msgstr "Północ!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.NE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.NE"
msgid "Northeast!"
msgstr "Północny wschód!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.NW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.NW"
msgid "Northwest!"
msgstr "Północny zachód!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.S
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.S"
msgid "South!"
msgstr "Południe!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.SE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.SE"
msgid "Southeast!"
msgstr "Południowy wschód!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.SW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.SW"
msgid "Southwest!"
msgstr "Południowy zachód!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPASS.W
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPASS.W"
msgid "West!"
msgstr "Zachód!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPOST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPOST"
msgid "Yucky muck!"
msgstr "Fuuj! Ble!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.BALANCED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.BALANCED"
msgid "We think it's ready!"
msgstr "Chyba jesteśmy gotowi!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.BURNT"
msgid "Uh oh..."
msgstr "O-oł..."

#. CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.DRY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.DRY"
msgid "It's like sand!"
msgstr "Jest jak piasek!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.GENERIC"
msgid "Let's make food for the plants!"
msgstr "Zróbmy jedzonko dla roślinek!"

#. CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.WET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPOSTINGBIN.WET"
msgid "It's all wet and goopy."
msgstr "Mokre i paskudne."

#. CHARACTERS.WEBBER.DESCRIBE.COMPOSTWRAP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COMPOSTWRAP"
msgid "Double doodoo."
msgstr "Podwójna kupka."

#. CHARACTERS.WEBBER.DESCRIBE.CONSTRUCTION_PLANS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CONSTRUCTION_PLANS"
msgid "We should build this."
msgstr "Powinniśmy to wybudować."

#. CHARACTERS.WEBBER.DESCRIBE.COOKBOOK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKBOOK"
msgid "Now we'll never forget a recipe!"
msgstr "Już nigdy nie zapomnimy przepisu!"

#. CHARACTERS.WEBBER.DESCRIBE.COOKEDMANDRAKE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKEDMANDRAKE"
msgid "Cooked to death."
msgstr "Upieczona na śmierć."

#. CHARACTERS.WEBBER.DESCRIBE.COOKEDMEAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKEDMEAT"
msgid "Can't have any pudding if we don't eat it."
msgstr "Jak nie zjemy, nie dostaniemy deseru."

#. CHARACTERS.WEBBER.DESCRIBE.COOKEDMONSTERMEAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKEDMONSTERMEAT"
msgid "I don't know what everyone's complaining about."
msgstr "Dlaczego wszyscy tak na to narzekają?"

#. CHARACTERS.WEBBER.DESCRIBE.COOKEDSMALLMEAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKEDSMALLMEAT"
msgid "Meat treat."
msgstr "Mięsna uczta."

#. CHARACTERS.WEBBER.DESCRIBE.COOKIECUTTER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKIECUTTER"
msgid "Our boat's not a cookie!"
msgstr "Nasza łódka to nie ciasteczko!"

#. CHARACTERS.WEBBER.DESCRIBE.COOKIECUTTERHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKIECUTTERHAT"
msgid "We look like a hermit crab!"
msgstr "Wyglądamy jak krab pustelnik!"

#. CHARACTERS.WEBBER.DESCRIBE.COOKIECUTTERSHELL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKIECUTTERSHELL"
msgid "Ouch! It's spiky!"
msgstr "Auć! Kłuje!"

#. CHARACTERS.WEBBER.DESCRIBE.COOKPOT.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKPOT.BURNT"
msgid "Someone must have left the fire going."
msgstr "Ktoś chyba zapomniał zdjąć go z ognia."

#. CHARACTERS.WEBBER.DESCRIBE.COOKPOT.COOKING_LONG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKPOT.COOKING_LONG"
msgid "It won't be done for a while."
msgstr "Jeszcze trochę to potrwa."

#. CHARACTERS.WEBBER.DESCRIBE.COOKPOT.COOKING_SHORT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKPOT.COOKING_SHORT"
msgid "Almost ready!"
msgstr "Już prawie gotowe!"

#. CHARACTERS.WEBBER.DESCRIBE.COOKPOT.DONE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKPOT.DONE"
msgid "Supper is served."
msgstr "Kolację podano."

#. CHARACTERS.WEBBER.DESCRIBE.COOKPOT.EMPTY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COOKPOT.EMPTY"
msgid "Food goes in, other food comes out."
msgstr "Żeby coś wyjąć, trzeba coś włożyć."

#. CHARACTERS.WEBBER.DESCRIBE.COONTAIL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.COONTAIL"
msgid "I always liked pulling Whiskers' tail."
msgstr "Zawsze lubiłem ciągnąć Mruczka za ogon."

#. CHARACTERS.WEBBER.DESCRIBE.CORN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CORN"
msgid "Corn in the raw."
msgstr "Kuku!"

#. CHARACTERS.WEBBER.DESCRIBE.CORN_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CORN_COOKED"
msgid "Popping corn."
msgstr "Strzelające ziarenka."

#. CHARACTERS.WEBBER.DESCRIBE.CORN_OVERSIZED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CORN_OVERSIZED"
msgid "Can we make giant popcorn?"
msgstr "Możemy zrobić tonę popcornu?"

#. CHARACTERS.WEBBER.DESCRIBE.CORN_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CORN_SEEDS"
msgid "We could grow something with these."
msgstr "Można by z nich coś wyhodować."

#. CHARACTERS.WEBBER.DESCRIBE.CRABKING.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CRABKING.GENERIC"
msgid "He looks pretty crabby!"
msgstr "Wygląda uszczypliwie!"

#. CHARACTERS.WEBBER.DESCRIBE.CRABKING.INERT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CRABKING.INERT"
msgid "Someone forgot to decorate their sandcastle."
msgstr "Ktoś zapomniał ozdobić swój zamek z piasku."

#. CHARACTERS.WEBBER.DESCRIBE.CRABKING_CLAW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CRABKING_CLAW"
msgid "Aaah! Leave us alone!"
msgstr "Ach! Zostaw nas w spokoju!"

#. CHARACTERS.WEBBER.DESCRIBE.CRITTERLAB
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CRITTERLAB"
msgid "It looks warm in there."
msgstr "Wygląda przytulnie..."

#. CHARACTERS.WEBBER.DESCRIBE.CRITTER_DRAGONLING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CRITTER_DRAGONLING"
msgid "She's a monster just like me!"
msgstr "Jest potworem, tak jak ja."

#. CHARACTERS.WEBBER.DESCRIBE.CRITTER_EYEOFTERROR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.CRITTER_EYEOFTERROR"
