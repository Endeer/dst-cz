msgstr "Nie czułbym się w porządku, gdybym zachował to wszystko dla siebie."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.1
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.1"
msgid "Hnff..."
msgstr "Hnff..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.10
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.10"
msgid "Huff... huff..."
msgstr "Uff... uff..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.2
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.2"
msgid "No... Don't get up... Lucy..."
msgstr "Nie... Nie wstawiaj... Lucy..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.3
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.3"
msgid "Heavy... Eh...?"
msgstr "Ciężko... Ej...?"

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.4
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.4"
msgid "...oof..."
msgstr "...uff..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.5
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.5"
msgid "I'll keep... lumberin' along..."
msgstr "Będziemy... szli... nieprzerwanie..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.6
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.6"
msgid "No point... complainin'..."
msgstr "Nie ma... co... narzekać..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.7
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.7"
msgid "Work's... gotta get done..."
msgstr "Robota... sama się... nie zrobi..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.8
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.8"
msgid "Nothin' like... honest work..."
msgstr "Nie ma to... jak uczciwa... praca..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.9
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENCUMBERED.9"
msgid "I'm more of a chopper than a lifter..."
msgstr "Jestem bardziej drwalem niż tragarzem..."

#. CHARACTERS.WOODIE.ANNOUNCE_ENTER_DARK
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENTER_DARK"
msgid "Who turned out the lights?"
msgstr "Kto zgasił światło?"

#. CHARACTERS.WOODIE.ANNOUNCE_ENTER_LIGHT
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_ENTER_LIGHT"
msgid "Brilliance!"
msgstr "Wspaniale!"

#. CHARACTERS.WOODIE.ANNOUNCE_EYEOFTERROR_ARRIVE
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_EYEOFTERROR_ARRIVE"
msgid "That's not what I was looking for, eh!"
msgstr "Nie tego szukałem, ech!"

#. CHARACTERS.WOODIE.ANNOUNCE_EYEOFTERROR_FLYAWAY
msgctxt "CHARACTERS.WOODIE.ANNOUNCE_EYEOFTERROR_FLYAWAY"
