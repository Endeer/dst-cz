msgid "Mum never let me have these."
msgstr "A mamãe nunca me deixou comer essas."

#. CHARACTERS.WEBBER.DESCRIBE.PREMIUMWATERINGCAN.EMPTY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PREMIUMWATERINGCAN.EMPTY"
msgid "We'll have to fill it up somewhere."
msgstr "A gente vai ter que encher ele em algum lugar."

#. CHARACTERS.WEBBER.DESCRIBE.PREMIUMWATERINGCAN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PREMIUMWATERINGCAN.GENERIC"
msgid "Our watering can back home didn't look anything like that."
msgstr "O regador lá de casa não se parece nem um pouco com esse."

#. CHARACTERS.WEBBER.DESCRIBE.PUFFIN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUFFIN.GENERIC"
msgid "It looks like a nice bird."
msgstr "Parece um pássaro legal."

#. CHARACTERS.WEBBER.DESCRIBE.PUFFIN.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUFFIN.HELD"
msgid "I got you now."
msgstr "Agora eu te peguei."

#. CHARACTERS.WEBBER.DESCRIBE.PUFFIN.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUFFIN.SLEEPING"
msgid "Nighty night."
msgstr "Boa noite."

#. CHARACTERS.WEBBER.DESCRIBE.PUMPKIN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUMPKIN"
msgid "That's a huge pumpkin!"
msgstr "Que abóbora enorme!"

#. CHARACTERS.WEBBER.DESCRIBE.PUMPKINCOOKIE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUMPKINCOOKIE"
msgid "Yum!"
msgstr "Hmm!"

#. CHARACTERS.WEBBER.DESCRIBE.PUMPKINPIE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUMPKINPIE"
msgid "We'd like a big slice, please!"
msgstr "A gente quer uma fatia grande!"

#. CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_COOKED"
msgid "It's all warm now."
msgstr "Está toda quentinha agora."

#. CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_LANTERN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_LANTERN"
msgid "Just like we used to make at home!"
msgstr "Como costumávamos fazer em casa!"

#. CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_OVERSIZED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_OVERSIZED"
msgid "Let's carve a big scary face into it!"
msgstr "Vamos esculpir uma careta assustadora nela!"

#. CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PUMPKIN_SEEDS"
msgid "We could grow something with these."
msgstr "Poderíamos cultivar alguma coisa com isso."

#. CHARACTERS.WEBBER.DESCRIBE.PURPLEAMULET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PURPLEAMULET"
msgid "Did you hear something?"
msgstr "Você ouviu alguma coisa?"

#. CHARACTERS.WEBBER.DESCRIBE.PURPLEGEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PURPLEGEM"
msgid "I can see knowledge swimming inside."
msgstr "Consigo ver o conhecimento nadando lá dentro."

#. CHARACTERS.WEBBER.DESCRIBE.PURPLEMOONEYE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.PURPLEMOONEYE"
msgid "Now we won't need to leave a trail of breadcrumbs!"
msgstr "Agora a gente não vai precisar deixar uma trilha de migalhas de pão!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR.FULL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR.FULL"
msgid "We hope it's good."
msgstr "Esperamos que ainda esteja bom."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR.GENERIC"
msgid "I think it's hungry."
msgstr "Acho que está com fome."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_BOLLARD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_BOLLARD"
msgid "It's nice enough."
msgstr "É bem legal."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_IVY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_IVY"
msgid "Ivy grows everywhere."
msgstr "Hera cresce por tudo."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_QUEEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_QUEEN"
msgid "Wow. She looks beautiful."
msgstr "Uau. Ela parece linda."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_STATUE1
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_STATUE1"
msgid "It looks like the friendly goat we met."
msgstr "Parece com a cabra amistosa que encontramos."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_STATUE2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ALTAR_STATUE2"
msgid "A lotta goat statues around here."
msgstr "Tem muitas estátuas de bodes por aqui."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_BEEFALO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_BEEFALO"
msgid "Take it easy, grandpa."
msgstr "Calma aí, vovô."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_BERRYBUSH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_BERRYBUSH"
msgid "Aww... all the berries are gone now."
msgstr "Ahh... todas as bagas se foram."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_BOWL_SILVER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_BOWL_SILVER"
msgid "Now we have somewhere to put food."
msgstr "Agora temos onde colocar comida."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CARROT_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CARROT_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CASSEROLEDISH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CASSEROLEDISH"
msgid "We can cooks stuff in it."
msgstr "Podemos cozinhar coisas com ele."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CASSEROLEDISH_SMALL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CASSEROLEDISH_SMALL"
msgid "It's kinda small."
msgstr "É meio pequeno."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN1
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN1"
msgid "We could trade it for something small."
msgstr "Podíamos trocar isso por algo pequeno."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN2"
msgid "I wonder what we can buy with this."
msgstr "O que será que a gente pode comprar com isso?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN3
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN3"
msgid "We can buy a whole bunch of stuff with this."
msgstr "Podemos comprar um monte de coisas com isso."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN4
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_COIN4"
msgid "The nice lady said we need three of them."
msgstr "A senhora simpática disse que precisamos de três."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CRABMEAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CRABMEAT"
msgid "Crabs kinda look like spiders."
msgstr "Siris meio que se parecem com aranhas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CRABMEAT_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CRABMEAT_COOKED"
msgid "We don't want to eat it."
msgstr "Não queremos comer isso."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CRABTRAP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_CRABTRAP"
msgid "We can trap little crabs in this."
msgstr "Podemos prender os sirizinhos nisso."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FERN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FERN"
msgid "Maybe we could make a salad."
msgstr "Quem sabe a gente pudesse fazer uma salada."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FLOUR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FLOUR"
msgid "It's no good by itself."
msgstr "Não funciona sozinha."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOLIAGE_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOLIAGE_COOKED"
msgid "This probably doesn't count as a salad, huh."
msgstr "Isso provavelmente não conta como salada, né."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.GENERIC"
msgid "We can put it on the altar now."
msgstr "A gente pode colocar no altar agora."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.MATCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.MATCH"
msgid "The goat lady said this was what the Gnaw wanted."
msgstr "A dona cabra disse que era isso que o Gnaw queria."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.MATCH_BUT_SNACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.MATCH_BUT_SNACK"
msgid "This is what it wants, but it's kinda puny."
msgstr "É isso que ele quer, mas é meio pouco."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.MISMATCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD.MISMATCH"
msgid "We think the Gnaw wants something different."
msgstr "A gente acha que o Gnaw quer algo diferente."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD_BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_FOOD_BURNT"
msgid "It's okay. The next one will be better."
msgstr "Tudo bem. A próxima vai ser melhor."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GARLIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GARLIC"
msgid "Maxwell says you ward monsters away with it."
msgstr "Maxwell diz que você afasta monstros com ele."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GARLIC_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GARLIC_COOKED"
msgid "It didn't ward us off!"
msgstr "Não nos afastou!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GARLIC_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GARLIC_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GOATKID
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GOATKID"
msgid "Hi! I'm Webber!"
msgstr "Olá! Meu nome é Webber!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GOATMILK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GOATMILK"
msgid "Milk is good for our bones. Endo and exo!"
msgstr "Leite faz bem pra os ossos. Internos e externos!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GOATMUM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GOATMUM"
msgid "Does your kid want to play?"
msgstr "Ei, crianças, querem brincar?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL"
msgid "We have to be careful when we cook."
msgstr "Precisamos ter cuidado quando estivermos cozinhando."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL_ITEM"
msgid "We could use it to make yummy food."
msgstr "Podíamos usar pra fazer comida gostosinha."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL_SMALL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL_SMALL"
msgid "What should we cook?"
msgstr "O que preparamos?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL_SMALL_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_GRILL_SMALL_ITEM"
msgid "Let's find a place to put it."
msgstr "Vamos procurar um lugar onde colocar."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_HOE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_HOE"
msgid "I know how to use this!"
msgstr "Sei como usar isso!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_KEY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_KEY"
msgid "Cool! I wonder what it unlocks?"
msgstr "Legal! O que será que destrava?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_KEY_PARK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_KEY_PARK"
msgid "We wanna go to the park."
msgstr "A gente quer ir pro parque."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_LAMP_POST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_LAMP_POST"
msgid "It's the kind of lamp they have in the city."
msgstr "É um tipo de abajur que eles têm na cidade."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_LAMP_SHORT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_LAMP_SHORT"
msgid "We're almost as tall as this lamp."
msgstr "Somos quase tão altos quanto essa luz."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MEALINGSTONE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MEALINGSTONE"
msgid "We want to do the grinding!"
msgstr "Queremos moer!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MERMHOUSE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MERMHOUSE"
msgid "It doesn't look very cozy."
msgstr "Não parece muito aconchegante."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MERM_CART1
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MERM_CART1"
msgid "Do they have any toys?"
msgstr "Eles têm algum brinquedo?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MERM_CART2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MERM_CART2"
msgid "That's full of good stuff."
msgstr "Isso está cheio de coisa boa."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MUSHROOMS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MUSHROOMS"
msgid "I don't like mushrooms."
msgstr "Não gosto de cogumelos."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MUSHROOMSTUMP.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MUSHROOMSTUMP.GENERIC"
msgid "Those mushrooms are tiny."
msgstr "Esses cogumelos são pequenos."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MUSHROOMSTUMP.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_MUSHROOMSTUMP.PICKED"
msgid "No more little mushrooms."
msgstr "Chega de cogumelos pequenos."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ONION
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ONION"
msgid "It makes all our eyes water!"
msgstr "Fazem todos nossos olhos lacrimejarem!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ONION_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ONION_COOKED"
msgid "Our eyes don't water any more."
msgstr "Nossos olhos não lacrimejam mais."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ONION_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ONION_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_OVEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_OVEN"
msgid "Mom said I should be careful around the oven."
msgstr "A mamãe disse que eu devia ter cuidado perto do forno."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_OVEN_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_OVEN_ITEM"
msgid "We gotta set this up."
msgstr "Precisamos montar."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARKSPIKE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARKSPIKE"
msgid "It looks sharp."
msgstr "Parece afiado."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_ANGEL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_ANGEL"
msgid "I think it's sort of pretty."
msgstr "Acho que é até meio bonita."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_ANGEL2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_ANGEL2"
msgid "It almost looks real."
msgstr "Quase parece real."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_FOUNTAIN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_FOUNTAIN"
msgid "No coins in it. I guess no one made wishes."
msgstr "Nenhuma moeda dentro. Acho que ninguém fez pedidos."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_GATE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_GATE.GENERIC"
msgid "Park. Park. Park!"
msgstr "Parque. Parque. Parque!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_GATE.LOCKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_GATE.LOCKED"
msgid "It's locked. I think we need a key."
msgstr "Está trancado. Acho que precisamos de uma chave."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_OBELISK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_OBELISK"
msgid "It's a monument or something."
msgstr "É um monumento ou algo assim."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_URN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PARK_URN"
msgid "Aw. Someone died."
msgstr "Oun. Alguém morreu."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PEBBLECRAB
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PEBBLECRAB"
msgid "Don't be afraid!"
msgstr "Não fique com medo!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PIGEON.DEAD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PIGEON.DEAD"
msgid "It's not moving."
msgstr "Não está se movendo."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PIGEON.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PIGEON.GENERIC"
msgid "I should leave it alone."
msgstr "É melhor eu não mexer."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PIGEON.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PIGEON.SLEEPING"
msgid "It's sleeping."
msgstr "Está dormindo."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PLATE_SILVER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PLATE_SILVER"
msgid "Now we don't have to eat off the floor."
msgstr "Agora a gente não precisa comer no chão."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POND_SALT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POND_SALT"
msgid "It's all crusty around the edges."
msgstr "É meio duro nas bordas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PORTAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PORTAL"
msgid "At least it took us somewhere different."
msgstr "Pelo menos nos levou pra algum lugar diferente."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PORTAL_KEY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_PORTAL_KEY"
msgid "I wonder where this takes us."
msgstr "Pra onde será que nos leva?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT"
msgid "This pot's a bit bigger than the other one."
msgstr "Essa panela é um pouco maior que a outra."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POTATO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POTATO"
msgid "Woah! A potato!"
msgstr "Uau! Uma batata!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POTATO_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POTATO_COOKED"
msgid "It's a cooked potato now."
msgstr "É uma batata assada agora."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POTATO_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POTATO_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_HANGER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_HANGER"
msgid "We can hang a pot off this."
msgstr "A gente pode pendurar uma panela nisso."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_HANGER_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_HANGER_ITEM"
msgid "We should put it together."
msgstr "Acho que devemos montar."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_SMALL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_SMALL"
msgid "You cook stuff in it."
msgstr "Você cozinha coisas nisso."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_SYRUP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_POT_SYRUP"
msgid "Sweet pot!"
msgstr "Pela panela!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ROTTEN_CROP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_ROTTEN_CROP"
msgid "That one got a bit squishy."
msgstr "Ficou meio mole."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_BIKE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_BIKE"
msgid "Aww... It's broken."
msgstr "Ahh... Está quebrada."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CARRIAGE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CARRIAGE"
msgid "I wish we could ride in that."
msgstr "Queria que a gente pudesse dar uma voltinha."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CATHEDRAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CATHEDRAL"
msgid "We can't even fix it."
msgstr "Não dá nem pra consertar."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CHIMNEY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CHIMNEY"
msgid "Bummer."
msgstr "Deu ruim!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CHIMNEY2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CHIMNEY2"
msgid "Aw. That's too bad."
msgstr "Ah. Que pena."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CLOCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CLOCK"
msgid "Does it still tell the right time?"
msgstr "Ainda mostra a hora certa?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CLOCKTOWER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_CLOCKTOWER"
msgid "I don't think that clock works anymore."
msgstr "Acho que esse relógio não funciona mais."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_HOUSE.1
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_HOUSE.1"
msgid "Where did everyone go?"
msgstr "Pra onde foi todo mundo?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_HOUSE.2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_HOUSE.2"
msgid "I wonder what happened here?"
msgstr "O que será que aconteceu aqui?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_HOUSE.3
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_HOUSE.3"
msgid "Looks like everyone left."
msgstr "Parece que todo mundo foi embora."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_PUBDOOR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_PUBDOOR"
msgid "It doesn't lead anywhere anymore."
msgstr "Não leva pra lugar nenhum mais."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_ROOF
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_RUBBLE_ROOF"
msgid "Too bad."
msgstr "Que pena."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAFE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAFE.GENERIC"
msgid "Well, it wouldn't hurt to take a look."
msgstr "Bem, não tiraria pedaço só olhar."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAFE.LOCKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAFE.LOCKED"
msgid "Someone's precious things are probably inside."
msgstr "As preciosidades de alguém provavelmente estão lá dentro."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALMON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALMON"
msgid "Mom said fish oil is good for our brain."
msgstr "A mamãe disse que óleo é bom pro cérebro."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALMON_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALMON_COOKED"
msgid "Smells good!"
msgstr "Tem um cheiro bom!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT"
msgid "We don't use a lot of salt on our food."
msgstr "Não usamos muito sal na nossa comida."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALTROCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALTROCK"
msgid "We're gonna crunch them all up."
msgstr "Vamos esmagar todas elas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT_RACK.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT_RACK.GENERIC"
msgid "We have to wait for more salt."
msgstr "A gente precisa esperar por mais sal."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT_RACK.READY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT_RACK.READY"
msgid "Salt's ready."
msgstr "O sal está pronto."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT_RACK_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SALT_RACK_ITEM"
msgid "We should set it up at the crusty pond."
msgstr "Devíamos colocar no lago duro."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAP"
msgid "Sticky, gooey sap."
msgstr "Seiva grudenta e gosmenta."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAPBUCKET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAPBUCKET"
msgid "It's a big ol'bucket."
msgstr "É um balde velho."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAPLING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAPLING"
msgid "It's gonna take too long to grow back."
msgstr "Vai demorar muito pra crescer de novo."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAP_SPOILED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SAP_SPOILED"
msgid "Aw... It's no good anymore."
msgstr "Ah... Está passado."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SEEDPACKET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SEEDPACKET"
msgid "We can plant a whole bunch of food with this."
msgstr "Podemos plantar vários alimentos com isso."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SLAUGHTERTOOL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SLAUGHTERTOOL"
msgid "I don't wanna use this."
msgstr "Não quero usar isso."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_GROUND
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_GROUND"
msgid "We ground it all up."
msgstr "Vamos moer tudo."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_SHRUB.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_SHRUB.GENERIC"
msgid "What a cool purple shrub."
msgstr "Que arbustinho roxo bacana."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_SHRUB.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_SHRUB.PICKED"
msgid "All the good stuff's gone."
msgstr "Toda a parte boa se foi."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_SPRIG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SPOTSPICE_SPRIG"
msgid "It doesn't taste very good on its own."
msgstr "Não tem um gosto muito bom sozinho."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.GENERIC"
msgid "I wonder why the people left this nice place."
msgstr "Por que será que as pessoas foram embora desse lugar?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.STUMP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.STUMP"
msgid "Why would anyone do that?"
msgstr "Por que alguém faria isso?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.TAPPED_BUGS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.TAPPED_BUGS"
msgid "Ew... The bugs got in it."
msgstr "Eca... Os insetos entraram."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.TAPPED_EMPTY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.TAPPED_EMPTY"
msgid "There isn't any sap yet."
msgstr "Ainda não tem seiva."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.TAPPED_READY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.TAPPED_READY"
msgid "Sticky, sticky sap!"
msgstr "Seiva grudenta, grudenta."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.WOUNDED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SUGARWOODTREE.WOUNDED"
msgid "Aw, now it's not as pink."
msgstr "Ah, agora não está tão rosa."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIG"
msgid "They're not afraid of us!!"
msgstr "Eles não têm medo de nós!!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIGELDER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIGELDER.GENERIC"
msgid "Hi, Mr. Pig!"
msgstr "Olá, sr. Porco!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIGELDER.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIGELDER.SLEEPING"
msgid "It's sleeping."
msgstr "Está dormindo."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE"
msgid "I've never seen their houses up close!"
msgstr "Nunca vi as casas deles de perto!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE_RUBBLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SWAMPIG_HOUSE_RUBBLE"
msgid "No one lives in it anymore."
msgstr "Ninguém mais vive aqui."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SYRUP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_SYRUP"
msgid "Sweet!"
msgstr "Doçura!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TOMATO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TOMATO"
msgid "Is it a fruit or a vegetable?"
msgstr "É uma fruta ou um legume?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TOMATO_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TOMATO_COOKED"
msgid "It's a cooked fregetable. Vruit?"
msgstr "É uma fregume cozido. Lruta?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TOMATO_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TOMATO_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TRADER_MERM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TRADER_MERM"
msgid "Hi there! Wanna trade?"
msgstr "Olá! Quer fazer negócio?"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TRADER_MERM2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TRADER_MERM2"
msgid "Hey. You look nice."
msgstr "Ei. Você parece muito bem."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TURNIP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TURNIP"
msgid "Wow! It's a turnip!"
msgstr "Uau! É um nabo!"

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TURNIP_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TURNIP_COOKED"
msgid "We cooked the turnip."
msgstr "Cozinhamos o nabo."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TURNIP_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_TURNIP_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_WHEAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_WHEAT"
msgid "If only I were back at the mill."
msgstr "Se pelo menos eu estivesse no moinho."

#. CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_WHEAT_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.QUAGMIRE_WHEAT_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.RABBIT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RABBIT.GENERIC"
msgid "We just want to play!"
msgstr "Só queremos brincar!"

#. CHARACTERS.WEBBER.DESCRIBE.RABBIT.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RABBIT.HELD"
msgid "He's ours now."
msgstr "Agora ele é nosso."

#. CHARACTERS.WEBBER.DESCRIBE.RABBITHOLE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RABBITHOLE.GENERIC"
msgid "Come out! We just want to be friends."
msgstr "Saia! Só queremos ser amigos."

#. CHARACTERS.WEBBER.DESCRIBE.RABBITHOLE.SPRING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RABBITHOLE.SPRING"
msgid "I hope they're okay in there."
msgstr "Espero que estejam bem lá dentro."

#. CHARACTERS.WEBBER.DESCRIBE.RABBITHOUSE.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RABBITHOUSE.BURNT"
msgid "A little overdone and overlarge."
msgstr "Meio queimado e meio grande."

#. CHARACTERS.WEBBER.DESCRIBE.RABBITHOUSE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RABBITHOUSE.GENERIC"
msgid "Just like grandpa said, \"You live in what you eat.\""
msgstr "Como o vovô dizia: \"Você vive no que come\"."

#. CHARACTERS.WEBBER.DESCRIBE.RAINCOAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RAINCOAT"
msgid "Dry fur is the best fur."
msgstr "A pele seca é a melhor pele."

#. CHARACTERS.WEBBER.DESCRIBE.RAINHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RAINHAT"
msgid "It'll keep the water out of your fur."
msgstr "Vai deixar a água longe de nosso pelo."

#. CHARACTERS.WEBBER.DESCRIBE.RAINOMETER.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RAINOMETER.BURNT"
msgid "I don't think that's accurate."
msgstr "Não acho que isso é preciso."

#. CHARACTERS.WEBBER.DESCRIBE.RAINOMETER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RAINOMETER.GENERIC"
msgid "It must be powered by magic."
msgstr "Deve ser movido a magia."

#. CHARACTERS.WEBBER.DESCRIBE.RATATOUILLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RATATOUILLE"
msgid "Do we have to eat our veggies?"
msgstr "Precisamos comer os verdes?"

#. CHARACTERS.WEBBER.DESCRIBE.RAZOR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RAZOR"
msgid "I watched my father use one of these."
msgstr "Eu vi meu pai usar uma dessas."

#. CHARACTERS.WEBBER.DESCRIBE.REDGEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REDGEM"
msgid "It feels warm, even on the coldest nights."
msgstr "É quentinho, mesmo nas noites mais frias."

#. CHARACTERS.WEBBER.DESCRIBE.REDLANTERN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REDLANTERN"
msgid "Our own personal night light!"
msgstr "Nossa luz noturna pessoal!"

#. CHARACTERS.WEBBER.DESCRIBE.REDMOONEYE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REDMOONEYE"
msgid "That rock needs a nap. Its eye is all red!"
msgstr "Essa rocha precisa de um cochilo. Seu olho está bem vermelho!"

#. CHARACTERS.WEBBER.DESCRIBE.REDPOUCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REDPOUCH"
msgid "We're so lucky!"
msgstr "A gente é muito sortudo!"

#. CHARACTERS.WEBBER.DESCRIBE.RED_CAP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RED_CAP"
msgid "It smells funny."
msgstr "Tem um cheiro esquisito."

#. CHARACTERS.WEBBER.DESCRIBE.RED_CAP_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RED_CAP_COOKED"
msgid "It smells better now."
msgstr "O cheiro está melhor agora."

#. CHARACTERS.WEBBER.DESCRIBE.RED_MUSHROOM.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RED_MUSHROOM.GENERIC"
msgid "Ready for the taking!"
msgstr "Pronto para ser colhido!"

#. CHARACTERS.WEBBER.DESCRIBE.RED_MUSHROOM.INGROUND
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RED_MUSHROOM.INGROUND"
msgid "We can't get at it like that."
msgstr "Não podemos pegar assim."

#. CHARACTERS.WEBBER.DESCRIBE.RED_MUSHROOM.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RED_MUSHROOM.PICKED"
msgid "I think some spores remain."
msgstr "Acho que alguns esporos ficaram."

#. CHARACTERS.WEBBER.DESCRIBE.REEDS.BURNING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REEDS.BURNING"
msgid "Those burn quickly!"
msgstr "Eles queimam rápido!"

#. CHARACTERS.WEBBER.DESCRIBE.REEDS.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REEDS.GENERIC"
msgid "I bet those would be useful."
msgstr "Aposto que iam ser úteis."

#. CHARACTERS.WEBBER.DESCRIBE.REEDS.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REEDS.PICKED"
msgid "Only stems remain."
msgstr "Só os caules sobraram."

#. CHARACTERS.WEBBER.DESCRIBE.REFINED_DUST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REFINED_DUST"
msgid "It's a good thing neither of us are allergic."
msgstr "Que bom que nenhum de nós é alérgico."

#. CHARACTERS.WEBBER.DESCRIBE.REFLECTIVEVEST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REFLECTIVEVEST"
msgid "Safety first!"
msgstr "Segurança em primeiro lugar!"

#. CHARACTERS.WEBBER.DESCRIBE.RELIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RELIC"
msgid "These haven't been used in a while."
msgstr "Faz tempo que isso não é usado."

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB.BURNT"
msgid "I think it's broken."
msgstr "Acho que está quebrado."

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB.GENERIC"
msgid "It's like a science lab in here!"
msgstr "Parece um laboratório de ciências aqui!"

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB2.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB2.BURNT"
msgid "All our work... gone."
msgstr "Todo o nosso trabalho... já era."

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB2.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB2.GENERIC"
msgid "Father used to work on something like that."
msgstr "O pai trabalhava em um lugar parecido com esse."

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB3.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB3.BURNT"
msgid "The fires care not for magic."
msgstr "Os fogos não se importam com magia."

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB3.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB3.GENERIC"
msgid "We should be careful around that."
msgstr "Precisamos ter cuidado perto disso."

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB4.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB4.BURNT"
msgid "Smells like cooking."
msgstr "Tem cheiro de cozimento."

#. CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB4.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESEARCHLAB4.GENERIC"
msgid "Did I just hear a squeak?"
msgstr "Será que eu ouvi um gritinho?"

#. CHARACTERS.WEBBER.DESCRIBE.RESKIN_TOOL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESKIN_TOOL"
msgid "Awww do we have to clean?"
msgstr "Ahhh, a gente precisa mesmo limpar?"

#. CHARACTERS.WEBBER.DESCRIBE.RESURRECTIONSTATUE.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESURRECTIONSTATUE.BURNT"
msgid "It will be of no use to us in that state."
msgstr "Não terá a mínima utilidade nesse estado."

#. CHARACTERS.WEBBER.DESCRIBE.RESURRECTIONSTATUE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESURRECTIONSTATUE.GENERIC"
msgid "It's a giant doll of our friend!!"
msgstr "É um boneco gigante do nosso amigo!!"

#. CHARACTERS.WEBBER.DESCRIBE.RESURRECTIONSTONE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RESURRECTIONSTONE"
msgid "Some sort of religious monument maybe?"
msgstr "Será que é algum tipo de monumento religioso?"

#. CHARACTERS.WEBBER.DESCRIBE.REVIVER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.REVIVER"
msgid "I think it loves me."
msgstr "Acho que me ama."

#. CHARACTERS.WEBBER.DESCRIBE.RHINODRILL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RHINODRILL"
msgid "Looks like a big bully!"
msgstr "Parece um grande intimidador!"

#. CHARACTERS.WEBBER.DESCRIBE.ROASTTURKEY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROASTTURKEY"
msgid "Can we have the wishbone?"
msgstr "A gente pode ficar com o ossinho da sorte?"

#. CHARACTERS.WEBBER.DESCRIBE.ROBIN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROBIN.GENERIC"
msgid "Red is my favorite color!"
msgstr "Vermelho é a minha cor favorita!"

#. CHARACTERS.WEBBER.DESCRIBE.ROBIN.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROBIN.HELD"
msgid "Held snug in our pockets."
msgstr "Bem aconchegado nos nossos bolsos."

#. CHARACTERS.WEBBER.DESCRIBE.ROBIN_WINTER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROBIN_WINTER.GENERIC"
msgid "What pretty white feathers."
msgstr "Que penas brancas bonitas."

#. CHARACTERS.WEBBER.DESCRIBE.ROBIN_WINTER.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROBIN_WINTER.HELD"
msgid "Let's name it \"Francis\"."
msgstr "Seu nome será \"Francis\"."

#. CHARACTERS.WEBBER.DESCRIBE.ROBOT_BLUEPRINT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROBOT_BLUEPRINT"
msgid "We hope it's not too heavy for the beefalo."
msgstr "Tomara que não seja pesada demais pro bífalo."

#. CHARACTERS.WEBBER.DESCRIBE.ROBOT_PUPPET
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROBOT_PUPPET"
msgid "I don't think they're having fun."
msgstr "Acho que não estão se divertindo."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK"
msgid "We'll need to mine it before we can use it."
msgstr "Precisaremos minerar antes de poder usar."

#. CHARACTERS.WEBBER.DESCRIBE.ROCKS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCKS"
msgid "None of these look like they would skip well."
msgstr "Nenhuma delas parece que saltaria bem."

#. CHARACTERS.WEBBER.DESCRIBE.ROCKY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCKY"
msgid "Snip snap!"
msgstr "Corta, corta!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.BARREN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.BARREN"
msgid "We don't think it can make fruit anymore."
msgstr "A gente acha que ela não produz mais frutas..."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.BURNING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.BURNING"
msgid "It's burning!"
msgstr "Tá queimando!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.DISEASED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.DISEASED"
msgid "It's really sick."
msgstr "Tá muito doente."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.DISEASING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.DISEASING"
msgid "It looks a little pale."
msgstr "Parece meio pálido."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.GENERIC"
msgid "There's rocks growing out of that bush!"
msgstr "Aquele arbusto dá frutas rochosas!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.PICKED"
msgid "It doesn't have anything good."
msgstr "Não tem nada de bom pra oferecer."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.WITHERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_BUSH.WITHERED"
msgid "Too hot."
msgstr "Muito quente."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT"
msgid "We have to wait for it to ripen."
msgstr "A gente precisa esperar que amadureça."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT_RIPE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT_RIPE"
msgid "We cracked it open."
msgstr "A gente conseguiu abrir."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT_RIPE_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT_RIPE_COOKED"
msgid "No more pit!"
msgstr "Não tem mais caroço!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT_SPROUT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_AVOCADO_FRUIT_SPROUT"
msgid "Oh wow, it's growing!"
msgstr "Ah, uau, tá crescendo!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_ICE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_ICE.GENERIC"
msgid "Ice can be useful."
msgstr "Gelo pode ser útil."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_ICE.MELTED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_ICE.MELTED"
msgid "Puddle!"
msgstr "Poça!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_ICE_MELTED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_ICE_MELTED"
msgid "Puddle!"
msgstr "Poça!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.GENERIC"
msgid "This lava's all dried up."
msgstr "Essa lava está completamente seca."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.LOW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.LOW"
msgid "It's a little less cozy."
msgstr "É um pouco menos aconchegante."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.NORMAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.NORMAL"
msgid "Cozy!"
msgstr "Aconchegante!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.OUT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_LIGHT.OUT"
msgid "It looks like it might break."
msgstr "Parece que vai quebrar."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_MOON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_MOON"
msgid "Neat!"
msgstr "Bacana!"

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_PETRIFIED_TREE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_PETRIFIED_TREE"
msgid "It's all stone and no bark."
msgstr "Foi completamente petrificada."

#. CHARACTERS.WEBBER.DESCRIBE.ROCK_PETRIFIED_TREE_OLD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROCK_PETRIFIED_TREE_OLD"
msgid "It's all stone and no bark."
msgstr "Foi completamente petrificada."

#. CHARACTERS.WEBBER.DESCRIBE.ROOK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROOK"
msgid "Who made these things!?"
msgstr "Quem fez isso?!"

#. CHARACTERS.WEBBER.DESCRIBE.ROOK_NIGHTMARE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROOK_NIGHTMARE"
msgid "You can't rook us."
msgstr "Você não nos engana."

#. CHARACTERS.WEBBER.DESCRIBE.ROPE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROPE"
msgid "We could tie stuff up with this."
msgstr "Poderíamos amarrar coisas com isso."

#. CHARACTERS.WEBBER.DESCRIBE.ROTTENEGG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROTTENEGG"
msgid "Ew!"
msgstr "Eca!"

#. CHARACTERS.WEBBER.DESCRIBE.ROYAL_JELLY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.ROYAL_JELLY"
msgid "It's goopy."
msgstr "É melequenta."

#. CHARACTERS.WEBBER.DESCRIBE.RUBBLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RUBBLE"
msgid "Rocks from an old city."
msgstr "Pedras de uma cidade antiga."

#. CHARACTERS.WEBBER.DESCRIBE.RUINSHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RUINSHAT"
msgid "And now we are king."
msgstr "E agora somos rei."

#. CHARACTERS.WEBBER.DESCRIBE.RUINS_BAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RUINS_BAT"
msgid "We will, we will, smash you!"
msgstr "Vamos, vamos, te esmagar!"

#. CHARACTERS.WEBBER.DESCRIBE.RUINS_RUBBLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.RUINS_RUBBLE"
msgid "It looks broken."
msgstr "Parece quebrado."

#. CHARACTERS.WEBBER.DESCRIBE.SACRED_CHEST.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SACRED_CHEST.GENERIC"
msgid "We feel cold."
msgstr "A gente está com frio."

#. CHARACTERS.WEBBER.DESCRIBE.SACRED_CHEST.LOCKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SACRED_CHEST.LOCKED"
msgid "It's judging us."
msgstr "Está julgando a gente."

#. CHARACTERS.WEBBER.DESCRIBE.SADDLEHORN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SADDLEHORN"
msgid "Leaves the beast nakey."
msgstr "Deixa a fera pelada."

#. CHARACTERS.WEBBER.DESCRIBE.SADDLE_BASIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SADDLE_BASIC"
msgid "We ride!"
msgstr "Vamos cavalgar!"

#. CHARACTERS.WEBBER.DESCRIBE.SADDLE_RACE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SADDLE_RACE"
msgid "It's a saddle made out of spidersnacks!"
msgstr "É uma sela feita de lanchinho de aranha!"

#. CHARACTERS.WEBBER.DESCRIBE.SADDLE_WAR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SADDLE_WAR"
msgid "We'll have lots of fun riding on this."
msgstr "A gente vai se divertir muito cavalgando nisso."

#. CHARACTERS.WEBBER.DESCRIBE.SALSA
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SALSA"
msgid "Yummy but... spicy!!"
msgstr "Gostosa, mas... apimentada!!"

#. CHARACTERS.WEBBER.DESCRIBE.SALTBOX
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SALTBOX"
msgid "Makes our food last longer."
msgstr "Faz nossa comida durar mais."

#. CHARACTERS.WEBBER.DESCRIBE.SALTLICK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SALTLICK"
msgid "We really regret licking it. Blech."
msgstr "A gente está muito arrependido de lamber isso. Blerg."

#. CHARACTERS.WEBBER.DESCRIBE.SALTROCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SALTROCK"
msgid "What a weird rock."
msgstr "Que rocha estranha."

#. CHARACTERS.WEBBER.DESCRIBE.SALTSTACK.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SALTSTACK.GENERIC"
msgid "They're kind of spooky."
msgstr "Elas são meio apavorantes."

#. CHARACTERS.WEBBER.DESCRIBE.SALTSTACK.GROWING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SALTSTACK.GROWING"
msgid "Hey, it's growing back!"
msgstr "Ei, tá crescendo de novo!"

#. CHARACTERS.WEBBER.DESCRIBE.SALTSTACK.MINED_OUT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SALTSTACK.MINED_OUT"
msgid "Someone already took all the salt."
msgstr "Alguém já pegou todo o sal."

#. CHARACTERS.WEBBER.DESCRIBE.SANDBLOCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SANDBLOCK"
msgid "Not like this!!"
msgstr "Assim, não!"

#. CHARACTERS.WEBBER.DESCRIBE.SANDSPIKE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SANDSPIKE"
msgid "Ouchie!"
msgstr "Ai!"

#. CHARACTERS.WEBBER.DESCRIBE.SANITYROCK.ACTIVE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SANITYROCK.ACTIVE"
msgid "It's in our way."
msgstr "Está no nosso caminho."

#. CHARACTERS.WEBBER.DESCRIBE.SANITYROCK.INACTIVE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SANITYROCK.INACTIVE"
msgid "Something about this rock feels off."
msgstr "Alguma coisa nessa pedra está estranha."

#. CHARACTERS.WEBBER.DESCRIBE.SAPLING.BURNING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SAPLING.BURNING"
msgid "So bright!"
msgstr "Que claro!"

#. CHARACTERS.WEBBER.DESCRIBE.SAPLING.DISEASED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SAPLING.DISEASED"
msgid "Maybe it needs some chicken soup?"
msgstr "Talvez precise de um pouco de caldo de galinha?"

#. CHARACTERS.WEBBER.DESCRIBE.SAPLING.DISEASING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SAPLING.DISEASING"
msgid "Are you okay, lil sapling?"
msgstr "Você não é muito falante, né?"

#. CHARACTERS.WEBBER.DESCRIBE.SAPLING.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SAPLING.GENERIC"
msgid "These sure grow slowly."
msgstr "Isso cresce devagar mesmo."

#. CHARACTERS.WEBBER.DESCRIBE.SAPLING.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SAPLING.PICKED"
msgid "Don't worry lil guy, it'll grow back!"
msgstr "Não se preocupe carinha, vai crescer de novo!"

#. CHARACTERS.WEBBER.DESCRIBE.SAPLING.WITHERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SAPLING.WITHERED"
msgid "I think the heat broke it."
msgstr "Acho que o calor a quebrou."

#. CHARACTERS.WEBBER.DESCRIBE.SCANDATA
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCANDATA"
msgid "Wow, it learned all that just by looking really hard?"
msgstr "Uau, ele aprendeu tudo isso só olhando muito intensamente?"

#. CHARACTERS.WEBBER.DESCRIBE.SCARECROW.BURNING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCARECROW.BURNING"
msgid "Someone should address this."
msgstr "Alguém deveria cuidar desse assunto."

#. CHARACTERS.WEBBER.DESCRIBE.SCARECROW.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCARECROW.BURNT"
msgid "He got dressed down."
msgstr "Ele foi despido."

#. CHARACTERS.WEBBER.DESCRIBE.SCARECROW.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCARECROW.GENERIC"
msgid "Let's play dress up!"
msgstr "Vamos brincar de se fantasiar!"

#. CHARACTERS.WEBBER.DESCRIBE.SCORCHED_SKELETON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCORCHED_SKELETON"
msgid "They're probably fine."
msgstr "Eles provavelmente estão bens."

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.BLOCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.BLOCK"
msgid "Oh! There's so many possibilities!"
msgstr "Oh! Há tantas possibilidades!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.BURNT"
msgid "Aww. Can we make another one?"
msgstr "Oun. A gente pode fazer outra?"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.EMPTY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.EMPTY"
msgid "I always wanted pottery lessons!"
msgstr "Sempre quis fazer aula de cerâmica!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.SCULPTURE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTINGTABLE.SCULPTURE"
msgid "Wow! It looks great!"
msgstr "Nossa! Parece ótima!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.COVERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.COVERED"
msgid "That looks really nice!"
msgstr "Isso parece bem legal!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.FINISHED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.FINISHED"
msgid "There! All better!"
msgstr "Agora sim! Bem melhor!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.READY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.READY"
msgid "I think it's stuck in there."
msgstr "Acho que está preso lá dentro."

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.UNCOVERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPBODY.UNCOVERED"
msgid "Where's your head, mister?"
msgstr "Onde está a sua cabeça, senhor?"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPHEAD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_BISHOPHEAD"
msgid "I think it's looking at us!"
msgstr "Acho que está olhando pra gente!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.COVERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.COVERED"
msgid "I think it really ties the island together."
msgstr "Acho que combina muito bem com a ilha."

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.FINISHED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.FINISHED"
msgid "You look great!"
msgstr "Você parece ótimo!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.READY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.READY"
msgid "I think it's stuck in there."
msgstr "Acho que está preso lá dentro."

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.UNCOVERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTBODY.UNCOVERED"
msgid "Peekaboo, monster!"
msgstr "Surpresa, monstro!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTHEAD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_KNIGHTHEAD"
msgid "Did someone lose this?"
msgstr "Alguém perdeu isso?"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.COVERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.COVERED"
msgid "It's a bit scary, but we don't like to judge."
msgstr "É meio assustador, mas a gente não gosta de julgar."

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.FINISHED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.FINISHED"
msgid "Oh! That piece was his nose."
msgstr "Oh! Aquela peça era o nariz."

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.READY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.READY"
msgid "I think it's stuck in there."
msgstr "Acho que está preso lá dentro."

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.UNCOVERED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKBODY.UNCOVERED"
msgid "It was hiding!"
msgstr "Estava se escondendo!"

#. CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKNOSE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SCULPTURE_ROOKNOSE"
msgid "Looks like a... cactus? No, that's not right."
msgstr "Parece um... cacto? Não, isso não está certo."

#. CHARACTERS.WEBBER.DESCRIBE.SEAFARING_PROTOTYPER.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEAFARING_PROTOTYPER.BURNT"
msgid "Fire is very dangerous."
msgstr "O fogo é muito perigoso."

#. CHARACTERS.WEBBER.DESCRIBE.SEAFARING_PROTOTYPER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEAFARING_PROTOTYPER.GENERIC"
msgid "There's all kinds of fun things you can do at sea."
msgstr "Você pode fazer todos os tipos de coisas divertidas no mar."

#. CHARACTERS.WEBBER.DESCRIBE.SEAFOODGUMBO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEAFOODGUMBO"
msgid "We could eat it forever!"
msgstr "Poderíamos comer para sempre!"

#. CHARACTERS.WEBBER.DESCRIBE.SEASTACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEASTACK"
msgid "We hope no one crashes their boat on that!"
msgstr "Tomara que ninguém bata com o barco nisso!"

#. CHARACTERS.WEBBER.DESCRIBE.SEEDPOUCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEEDPOUCH"
msgid "We're gonna fill it with seeds!"
msgstr "A gente vai encher de sementes!"

#. CHARACTERS.WEBBER.DESCRIBE.SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEEDS"
msgid "A small life trapped within. My sympathies."
msgstr "Uma vidinha presa no interior. Meus sentimentos."

#. CHARACTERS.WEBBER.DESCRIBE.SEEDS_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEEDS_COOKED"
msgid "Their growing days are over."
msgstr "Seus dias de crescimento acabaram."

#. CHARACTERS.WEBBER.DESCRIBE.SENTRYWARD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SENTRYWARD"
msgid "Woah! I bet it sees so far!"
msgstr "Ou! Aposto que enxerga bem longe!"

#. CHARACTERS.WEBBER.DESCRIBE.SEWING_KIT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEWING_KIT"
msgid "Mum used to handle all of our sewing."
msgstr "A mamãe costumava cuidar das costuras."

#. CHARACTERS.WEBBER.DESCRIBE.SEWING_TAPE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SEWING_TAPE"
msgid "It's sticky, like a web."
msgstr "É grudento, como uma teia."

#. CHARACTERS.WEBBER.DESCRIBE.SHADOWDIGGER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHADOWDIGGER"
msgid "Sometimes scary things are nice."
msgstr "Algumas vezes coisas assustadoras são legais."

#. CHARACTERS.WEBBER.DESCRIBE.SHADOWHEART
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHADOWHEART"
msgid "Why does it make me feel so bad?"
msgstr "Por que me faz sentir tão mal?"

#. CHARACTERS.WEBBER.DESCRIBE.SHARK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHARK"
msgid "Shark attack!"
msgstr "Ataque de tubarão!"

#. CHARACTERS.WEBBER.DESCRIBE.SHELL_CLUSTER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHELL_CLUSTER"
msgid "Any pretty shells in there?"
msgstr "Tem alguma concha bonita aí?"

#. CHARACTERS.WEBBER.DESCRIBE.SHIELDOFTERROR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHIELDOFTERROR"
msgid "Ms. Wickerbottom says we shouldn't bite."
msgstr "A Sra. Wickerbottom diz que não podemos morder."

#. CHARACTERS.WEBBER.DESCRIBE.SHOVEL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHOVEL"
msgid "Maybe we can tunnel our way out?"
msgstr "Quem sabe pudéssemos cavar um túnel de fuga?"

#. CHARACTERS.WEBBER.DESCRIBE.SHROOMCAKE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHROOMCAKE"
msgid "Hey, this isn't real cake!"
msgstr "Ei, isso não é bolo de verdade!"

#. CHARACTERS.WEBBER.DESCRIBE.SHROOM_SKIN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SHROOM_SKIN"
msgid "Haha! Yuck!"
msgstr "Ha, ha! Eca!"

#. CHARACTERS.WEBBER.DESCRIBE.SIESTAHUT.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SIESTAHUT.BURNT"
msgid "We can't take a nap in that!"
msgstr "Não podemos cochilar aí!"

#. CHARACTERS.WEBBER.DESCRIBE.SIESTAHUT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SIESTAHUT.GENERIC"
msgid "Is it nap time?"
msgstr "É hora da soneca?"

#. CHARACTERS.WEBBER.DESCRIBE.SILK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SILK"
msgid "It's so smooth!"
msgstr "É tão macia!"

#. CHARACTERS.WEBBER.DESCRIBE.SINGINGSHELL_OCTAVE3.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SINGINGSHELL_OCTAVE3.GENERIC"
msgid "We thought we saw something skitter inside it!"
msgstr "A gente acha que viu algo se mexendo dentro dele!"

#. CHARACTERS.WEBBER.DESCRIBE.SINGINGSHELL_OCTAVE4.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SINGINGSHELL_OCTAVE4.GENERIC"
msgid "It makes us want to hum along."
msgstr "Faz a gente querer cantar junto."

#. CHARACTERS.WEBBER.DESCRIBE.SINGINGSHELL_OCTAVE5.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SINGINGSHELL_OCTAVE5.GENERIC"
msgid "Do you know any songs?"
msgstr "Conhece alguma música?"

#. CHARACTERS.WEBBER.DESCRIBE.SISTURN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SISTURN.GENERIC"
msgid "It's a little Abigail house!"
msgstr "É uma casinha pra Abigail!"

#. CHARACTERS.WEBBER.DESCRIBE.SISTURN.LOTS_OF_FLOWERS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SISTURN.LOTS_OF_FLOWERS"
msgid "We feel warm and fuzzy... well, more than usual."
msgstr "A gente se sente quentinho e confortável... bom, mais do que o normal."

#. CHARACTERS.WEBBER.DESCRIBE.SISTURN.SOME_FLOWERS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SISTURN.SOME_FLOWERS"
msgid "We can find more flowers!"
msgstr "A gente pode achar mais flores!"

#. CHARACTERS.WEBBER.DESCRIBE.SKELETON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKELETON"
msgid "Hello? Are you alright?"
msgstr "Alô? Você está bem?"

#. CHARACTERS.WEBBER.DESCRIBE.SKELETONHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKELETONHAT"
msgid "It's not very comfy."
msgstr "Não é muito favorável."

#. CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.DEFAULT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.DEFAULT"
msgid "Oh no, %s! %s must have really hurt them!"
msgstr "Oh, não, %s! %s deve ter mesmo se machucado!"

#. CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.FEMALE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.FEMALE"
msgid "Oh no, %s! %s must have really hurt her!"
msgstr "Oh, não, %s! %s deve ter mesmo machucado ela!"

#. CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.MALE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.MALE"
msgid "Oh no, %s! %s must have really hurt him!"
msgstr "Oh, não, %s! %s deve ter mesmo machucado ele!"

#. CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.ROBOT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKELETON_PLAYER.ROBOT"
msgid "Oh no, %s! %s must have really hurt them!"
msgstr "Oh, não, %s! %s deve ter mesmo se machucado!"

#. CHARACTERS.WEBBER.DESCRIBE.SKETCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKETCH"
msgid "We need somewhere to make it!"
msgstr "A gente precisa de algo pra fazer isso!"

#. CHARACTERS.WEBBER.DESCRIBE.SKULLCHEST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SKULLCHEST"
msgid "Who knows what could be hiding in there!"
msgstr "Quem sabe o que poderia esta se escondendo aqui..."

#. CHARACTERS.WEBBER.DESCRIBE.SLEEPBOMB
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLEEPBOMB"
msgid "Throwable nap time!"
msgstr "Hora do cochilo arremessável!"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOT"
msgid "Can we play with it?"
msgstr "Podemos brincar com ele?"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_FREEZE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_FREEZE"
msgid "Do you want us to help clean up?"
msgstr "Você quer que ajudemos a limpar?"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_GOLD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_GOLD"
msgid "Do you want us to help clean up?"
msgstr "Você quer que ajudemos a limpar?"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_MARBLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_MARBLE"
msgid "Do you want us to help clean up?"
msgstr "Você quer que ajudemos a limpar?"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_POOP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_POOP"
msgid "Haha, ew!"
msgstr "Haha, eca!"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_ROCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_ROCK"
msgid "Do you want us to help clean up?"
msgstr "Você quer que ajudemos a limpar?"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_SLOW
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_SLOW"
msgid "Do you want us to help clean up?"
msgstr "Você quer que ajudemos a limpar?"

#. CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_THULECITE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLINGSHOTAMMO_THULECITE"
msgid "Do you want us to help clean up?"
msgstr "Você quer que ajudemos a limpar?"

#. CHARACTERS.WEBBER.DESCRIBE.SLURPER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURPER"
msgid "Hairy tongue thing!"
msgstr "Coisinha língua peluda!"

#. CHARACTERS.WEBBER.DESCRIBE.SLURPERPELT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURPERPELT"
msgid "It's like a tiny rug."
msgstr "É como se fosse um tapetinho."

#. CHARACTERS.WEBBER.DESCRIBE.SLURPER_PELT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURPER_PELT"
msgid "It's like a tiny rug."
msgstr "É como se fosse um tapetinho."

#. CHARACTERS.WEBBER.DESCRIBE.SLURTLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURTLE"
msgid "We'll slaughter that slurtle."
msgstr "Vamos trucidar essa tartareca."

#. CHARACTERS.WEBBER.DESCRIBE.SLURTLEHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURTLEHAT"
msgid "A solid helmet."
msgstr "Um capacete sólido."

#. CHARACTERS.WEBBER.DESCRIBE.SLURTLEHOLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURTLEHOLE"
msgid "Slimy and rocky."
msgstr "Viscoso e rochoso."

#. CHARACTERS.WEBBER.DESCRIBE.SLURTLESLIME
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURTLESLIME"
msgid "Slime-time!"
msgstr "Hora da meleca!"

#. CHARACTERS.WEBBER.DESCRIBE.SLURTLE_SHELLPIECES
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SLURTLE_SHELLPIECES"
msgid "Maybe that was a little too rough."
msgstr "Talvez tenha sido muito duro."

#. CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.GENERIC"
msgid "Could this be a friend for us?"
msgstr "Será que ele seria nosso amigo?"

#. CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.HUNGRY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.HUNGRY"
msgid "I can see its tummy rumble."
msgstr "Consigo ver sua barriga roncando."

#. CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.SLEEPING"
msgid "Shhh! It's dreaming."
msgstr "Shhh! Ele tá sonhando."

#. CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.STARVING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SMALLBIRD.STARVING"
msgid "Poor thing. It looks so hungry!"
msgstr "Pobrezinho. Parece faminto!"

#. CHARACTERS.WEBBER.DESCRIBE.SMALLGHOST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SMALLGHOST"
msgid "Hey little guy! Wanna play?"
msgstr "E aí, carinha! Quer brincar?"

#. CHARACTERS.WEBBER.DESCRIBE.SMALLMEAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SMALLMEAT"
msgid "A couple more'll make a morsel meal!"
msgstr "Mais algumas e teremos uma refeição de nacos de carne!"

#. CHARACTERS.WEBBER.DESCRIBE.SMALLMEAT_DRIED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SMALLMEAT_DRIED"
msgid "It'll keep longer this way."
msgstr "Vai durar mais assim."

#. CHARACTERS.WEBBER.DESCRIBE.SNAPPER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SNAPPER"
msgid "Don't bite us!"
msgstr "Não morde a gente!"

#. CHARACTERS.WEBBER.DESCRIBE.SNURTLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SNURTLE"
msgid "Let's snuff out that snurtle."
msgstr "Vamos farejar a tartareca."

#. CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER.GENERIC"
msgid "Wendy dared us to drink it..."
msgstr "A Wendy desafiou a gente a beber..."

#. CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER.SPOILED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER.SPOILED"
msgid "It's gotten really rotten looking... yay!"
msgstr "Ficou com um aspecto muito podre... eba!"

#. CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER.STALE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER.STALE"
msgid "Is it going bad? Is that good?"
msgstr "Tá ficando ruim? E isso é bom?"

#. CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER_FERMENTED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SOIL_AMENDER_FERMENTED"
msgid "It smells done to us!"
msgstr "Tá com cheirinho de pronta!"

#. CHARACTERS.WEBBER.DESCRIBE.SPAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPAT"
msgid "Maybe it just needs a cuddle!"
msgstr "Só precisa de um cafunezinho!"

#. CHARACTERS.WEBBER.DESCRIBE.SPEAR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPEAR"
msgid "We should stick things with the pointy part."
msgstr "Devíamos espetar coisas com a parte pontuda."

#. CHARACTERS.WEBBER.DESCRIBE.SPEAR_GUNGNIR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPEAR_GUNGNIR"
msgid "Wigfrid makes it look so cool!"
msgstr "Isso fica tão legal com a Wigfrid!"

#. CHARACTERS.WEBBER.DESCRIBE.SPEAR_LANCE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPEAR_LANCE"
msgid "I don't really want it."
msgstr "Não quero muito isso."

#. CHARACTERS.WEBBER.DESCRIBE.SPEAR_WATHGRITHR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPEAR_WATHGRITHR"
msgid "Pointy ouchies!"
msgstr "A ponta machuca!"

#. CHARACTERS.WEBBER.DESCRIBE.SPICEPACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPICEPACK"
msgid "I'm sure Mr. Warly won't mind if we use this!"
msgstr "O Warly não vai se importar se a gente usar isso!"

#. CHARACTERS.WEBBER.DESCRIBE.SPICE_CHILI
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPICE_CHILI"
msgid "Ho! Ha! That's spicy!"
msgstr "Ho! Ha! Que apimentados!"

#. CHARACTERS.WEBBER.DESCRIBE.SPICE_GARLIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPICE_GARLIC"
msgid "Our breath will smell stinky. Yay!"
msgstr "Nosso hálito vai ficar fedorento. Viva!"

#. CHARACTERS.WEBBER.DESCRIBE.SPICE_SALT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPICE_SALT"
msgid "Mmm, salty!"
msgstr "Mmm, salgadinho!"

#. CHARACTERS.WEBBER.DESCRIBE.SPICE_SUGAR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPICE_SUGAR"
msgid "It turns everything into dessert!"
msgstr "Transforma tudo em sobremesa!"

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER.DEAD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER.DEAD"
msgid "Another lost friend."
msgstr "Outra amiga perdida."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER.GENERIC"
msgid "Spiders understand us."
msgstr "As aranhas nos entendem."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER.SLEEPING"
msgid "Aww! I think it's having a dream."
msgstr "Oun! Acho que está sonhando."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDERDEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDERDEN"
msgid "Looks cozy in there."
msgstr "Parece aconchegante lá dentro."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDERDEN_BEDAZZLER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDERDEN_BEDAZZLER"
msgid "We know what'll make our spiderfriends happy, decorating their house!"
msgstr "A gente sabe o que vai deixar nossas amigaranhas felizes: decorar a casa delas!"

#. CHARACTERS.WEBBER.DESCRIBE.SPIDEREGGSACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDEREGGSACK"
msgid "A portable friendship pod!"
msgstr "Um recipiente de amizade portátil."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDERGLAND
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDERGLAND"
msgid "I won't think about where it came from."
msgstr "Não vou pensar de onde veio isso."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDERHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDERHAT"
msgid "Very upsetting..."
msgstr "Isso é muito triste..."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDERHOLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDERHOLE"
msgid "We could stand to live there."
msgstr "Poderíamos aguentar viver aqui."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDERHOLE_ROCK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDERHOLE_ROCK"
msgid "We could stand to live there."
msgstr "Poderíamos aguentar viver aqui."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDERQUEEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDERQUEEN"
msgid "Mommy-Longlegs?"
msgstr "Mamãe pernas compridas?"

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_DROPPER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_DROPPER"
msgid "Our friends live up there."
msgstr "Nossas amigas vivem lá."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_HEALER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_HEALER"
msgid "We feel better just having them around!"
msgstr "A gente se sente melhor só de ter elas por perto!"

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_HEALER_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_HEALER_ITEM"
msgid "It's good for fixing spider booboos."
msgstr "É boa pra curar dodói de aranha."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_HIDER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_HIDER"
msgid "Friends!"
msgstr "Amigos!"

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_MOON.DEAD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_MOON.DEAD"
msgid "Sorry!"
msgstr "Desculpe!"

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_MOON.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_MOON.GENERIC"
msgid "These friends look... weird."
msgstr "Essas amigas são... bizarras."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_MOON.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_MOON.SLEEPING"
msgid "Night-night, scary friend."
msgstr "Boa noite, amiga assustadora."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_REPELLENT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_REPELLENT"
msgid "Ms. Wickerbottom said our friends need to go home if they don't behave."
msgstr "A Sra. Wickerbottom disse que nossas amigas precisam ir embora se não se comportarem."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_SPITTER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_SPITTER"
msgid "Lay down some web for us."
msgstr "Lança umas teias para nós."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_WARRIOR.DEAD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_WARRIOR.DEAD"
msgid "Forgive us, brother."
msgstr "Desculpe a gente, irmã."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_WARRIOR.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_WARRIOR.GENERIC"
msgid "He will protect us!"
msgstr "Ele vai nos proteger!"

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_WARRIOR.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_WARRIOR.SLEEPING"
msgid "They're so cute when they sleep."
msgstr "Eles são tão bonitinhos quando estão dormindo."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_WATER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_WATER"
msgid "Maybe they can teach us how to swim."
msgstr "Talvez elas possam ensinar a gente a nadar."

#. CHARACTERS.WEBBER.DESCRIBE.SPIDER_WHISTLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPIDER_WHISTLE"
msgid "We can call our spider friends!"
msgstr "A gente pode chamar nossas amigaranhas!"

#. CHARACTERS.WEBBER.DESCRIBE.SPOILED_FISH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPOILED_FISH"
msgid "It's no good to anyone now."
msgstr "Não tem mais utilidade para ninguém."

#. CHARACTERS.WEBBER.DESCRIBE.SPOILED_FOOD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPOILED_FOOD"
msgid "Blech!"
msgstr "Blerg!"

#. CHARACTERS.WEBBER.DESCRIBE.SPORE_MEDIUM.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPORE_MEDIUM.GENERIC"
msgid "Our favorite color! Red!"
msgstr "Nossa cor favorita! Vermelho!"

#. CHARACTERS.WEBBER.DESCRIBE.SPORE_MEDIUM.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPORE_MEDIUM.HELD"
msgid "If we stare at it we'll turn to stone!"
msgstr "Se a gente encarar, vai virar pedra!"

#. CHARACTERS.WEBBER.DESCRIBE.SPORE_MOON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPORE_MOON"
msgid "They pop like balloons! Not as fun, though."
msgstr "Eles explodem como balões! Mas não são tão divertidos."

#. CHARACTERS.WEBBER.DESCRIBE.SPORE_SMALL.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPORE_SMALL.GENERIC"
msgid "That's green, our favoritest color!"
msgstr "É verde, nossa cor favorita!"

#. CHARACTERS.WEBBER.DESCRIBE.SPORE_SMALL.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPORE_SMALL.HELD"
msgid "If we lick it we'll turn to wood!"
msgstr "Se a gente lamber isso, vai virar madeira!"

#. CHARACTERS.WEBBER.DESCRIBE.SPORE_TALL.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPORE_TALL.GENERIC"
msgid "Blue! That's our favorite color!"
msgstr "Azul! Essa é a nossa cor favorita!"

#. CHARACTERS.WEBBER.DESCRIBE.SPORE_TALL.HELD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SPORE_TALL.HELD"
msgid "If we eat it we'll turn to water!"
msgstr "Se a gente comer isso, vai virar água!"

#. CHARACTERS.WEBBER.DESCRIBE.SQUID
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SQUID"
msgid "Wish we had eyes like that!"
msgstr "A gente queria ter olhos como esses!"

#. CHARACTERS.WEBBER.DESCRIBE.STAFFCOLDLIGHT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STAFFCOLDLIGHT"
msgid "Woah! The cold makes all our hairs stand up!"
msgstr "Uau! O frio arrepia nossos pelos!"

#. CHARACTERS.WEBBER.DESCRIBE.STAFFLIGHT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STAFFLIGHT"
msgid "Cozy."
msgstr "Aconchegante."

#. CHARACTERS.WEBBER.DESCRIBE.STAFF_TORNADO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STAFF_TORNADO"
msgid "We'll huff and we'll puff."
msgstr "Vamos bufar e vamos soprar."

#. CHARACTERS.WEBBER.DESCRIBE.STAGEHAND.AWAKE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STAGEHAND.AWAKE"
msgid "Definitely not a spider!"
msgstr "Definitivamente não é uma aranha!"

#. CHARACTERS.WEBBER.DESCRIBE.STAGEHAND.HIDING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STAGEHAND.HIDING"
msgid "Is there a spider friend rustling beneath?"
msgstr "Tem uma aranha amiga se mexendo aí embaixo?"

#. CHARACTERS.WEBBER.DESCRIBE.STALAGMITE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STALAGMITE"
msgid "Rocks, underground?! Shocking."
msgstr "Pedras no subterrâneo?! Chocante!"

#. CHARACTERS.WEBBER.DESCRIBE.STALAGMITE_TALL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STALAGMITE_TALL"
msgid "Pointy rocks, underground?! Simply stunning."
msgstr "Pedras pontudas no subterrâneo?! É genial."

#. CHARACTERS.WEBBER.DESCRIBE.STALKER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STALKER"
msgid "I don't think it's friendly."
msgstr "Acho que não é amistoso."

#. CHARACTERS.WEBBER.DESCRIBE.STALKER_ATRIUM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STALKER_ATRIUM"
msgid "We'll defend ourselves if we have to."
msgstr "Vamos nos defender se for preciso."

#. CHARACTERS.WEBBER.DESCRIBE.STALKER_MINION
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STALKER_MINION"
msgid "Creepy!"
msgstr "Assustador!"

#. CHARACTERS.WEBBER.DESCRIBE.STATUEGLOMMER.EMPTY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUEGLOMMER.EMPTY"
msgid "Take that, rock bug!"
msgstr "Toma essa, inseto pedra!"

#. CHARACTERS.WEBBER.DESCRIBE.STATUEGLOMMER.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUEGLOMMER.GENERIC"
msgid "A statue of some weird bug."
msgstr "Uma estátua de um inseto estranho."

#. CHARACTERS.WEBBER.DESCRIBE.STATUEHARP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUEHARP"
msgid "Someone took the head."
msgstr "Alguém levou a cabeça."

#. CHARACTERS.WEBBER.DESCRIBE.STATUEMAXWELL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUEMAXWELL"
msgid "We're still a little mad at him. But only a little."
msgstr "A gente ainda está zangado com ele. Mas só um pouquinho."

#. CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.GENERIC"
msgid "Solid marble!"
msgstr "Mármore sólido!"

#. CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.TYPE1
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.TYPE1"
msgid "Nice sword!"
msgstr "Bela espada!"

#. CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.TYPE2
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.TYPE2"
msgid "It looks real lifelike, but it's all cold to touch."
msgstr "Parece bem real, mas é gelado ao toque."

#. CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.TYPE3
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STATUE_MARBLE.TYPE3"
msgid "I broke mother's vase once... she wasn't happy."
msgstr "Eu quebrei um vaso da minha mãe... e ela não ficou muito feliz."

#. CHARACTERS.WEBBER.DESCRIBE.STEELWOOL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STEELWOOL"
msgid "Scratchy, like father's beard!"
msgstr "Áspero, como a barba do papai!"

#. CHARACTERS.WEBBER.DESCRIBE.STEERINGWHEEL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STEERINGWHEEL"
msgid "Oh, oh, we want to steer!"
msgstr "Ah, a gente quer dirigir!"

#. CHARACTERS.WEBBER.DESCRIBE.STEERINGWHEEL_ITEM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STEERINGWHEEL_ITEM"
msgid "That's for making a steering wheel."
msgstr "Isso é pra fazer um timão."

#. CHARACTERS.WEBBER.DESCRIBE.STINGER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STINGER"
msgid "We should be careful, we could poke an eye out!"
msgstr "Devemos ter cuidado, poderíamos arrancar um olho fora!"

#. CHARACTERS.WEBBER.DESCRIBE.STRAWHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STRAWHAT"
msgid "Keeps the sun out of all eight of your eyes."
msgstr "Mantém o sol longe dos nossos oito olhos."

#. CHARACTERS.WEBBER.DESCRIBE.STUFFEDEGGPLANT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STUFFEDEGGPLANT"
msgid "We will stuff ourself with eggyplants!"
msgstr "Vamos nos encher de berinjelas!"

#. CHARACTERS.WEBBER.DESCRIBE.STUFFING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.STUFFING"
msgid "It smells so good!"
msgstr "Cheira tão bem!"

#. CHARACTERS.WEBBER.DESCRIBE.SUCCULENT_PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SUCCULENT_PICKED"
msgid "It got picked."
msgstr "Foi colhida."

#. CHARACTERS.WEBBER.DESCRIBE.SUCCULENT_PLANT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SUCCULENT_PLANT"
msgid "We think it's a cactus."
msgstr "A gente acha que é um cacto."

#. CHARACTERS.WEBBER.DESCRIBE.SUCCULENT_POTTED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SUCCULENT_POTTED"
msgid "I wonder if mum's ever seen one."
msgstr "Será que a mamãe viu uma?"

#. CHARACTERS.WEBBER.DESCRIBE.SUNKENCHEST.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SUNKENCHEST.GENERIC"
msgid "Whew... we thought there might be an angry clam inside."
msgstr "Ufa... a gente achou que poderia ter um marisco furioso aí dentro."

#. CHARACTERS.WEBBER.DESCRIBE.SUNKENCHEST.LOCKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SUNKENCHEST.LOCKED"
msgid "It won't let us in!"
msgstr "Não quer abrir!"

#. CHARACTERS.WEBBER.DESCRIBE.SUPERTACKLECONTAINER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SUPERTACKLECONTAINER"
msgid "We can put way more fishing stuff in there!"
msgstr "A gente pode botar mais material de pesca dentro!"

#. CHARACTERS.WEBBER.DESCRIBE.SURFNTURF
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SURFNTURF"
msgid "Eww, healthy!"
msgstr "Uh, saudável!"

#. CHARACTERS.WEBBER.DESCRIBE.SWEATERVEST
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SWEATERVEST"
msgid "It itches and fits funny."
msgstr "Dá coceira e fica engraçado."

#. CHARACTERS.WEBBER.DESCRIBE.SWEETPOTATO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SWEETPOTATO"
msgid "Are those marshmallows on top?"
msgstr "Tem marshmallows em cima?"

#. CHARACTERS.WEBBER.DESCRIBE.SWEETTEA
msgctxt "CHARACTERS.WEBBER.DESCRIBE.SWEETTEA"
msgid "Mother used to make tea for me... at least I think she did..."
msgstr "Minha mãe sempre fazia chá pra mim... pelo menos eu acho que fazia..."

#. CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.BURNT"
msgid "Aww, someone ruined it."
msgstr "Ahh, alguém destruiu tudo."

#. CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.GENERIC"
msgid "So big it could fit everyone!"
msgstr "É tão grande que cabe todo mundo!"

#. CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.HAS_FOOD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.HAS_FOOD"
msgid "Enough food for everyone!"
msgstr "Tem comida suficiente para todos!"

#. CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.WRONG_TYPE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TABLE_WINTERS_FEAST.WRONG_TYPE"
msgid "Not the season for this."
msgstr "Não é a época para isso."

#. CHARACTERS.WEBBER.DESCRIBE.TACKLECONTAINER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TACKLECONTAINER"
msgid "It's like a toybox, but for fishing stuff!"
msgstr "É como uma caixa de brinquedos, só que para material de pesca!"

#. CHARACTERS.WEBBER.DESCRIBE.TACKLESKETCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TACKLESKETCH"
msgid "It has fishing secrets!"
msgstr "Contém segredos de pesca!"

#. CHARACTERS.WEBBER.DESCRIBE.TACKLESTATION
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TACKLESTATION"
msgid "Father used to take me fishing sometimes."
msgstr "Meu pai me levava para pescar de vez em quando."

#. CHARACTERS.WEBBER.DESCRIBE.TAFFY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TAFFY"
msgid "Candy!"
msgstr "Doce!"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRD"
msgid "Look at those legs!"
msgstr "Olha aquelas pernas!"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG"
msgid "Did I hear a meep?"
msgstr "Ouvi um pio?"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_COOKED"
msgid "Smells great!"
msgstr "O cheiro é ótimo!"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.COLD
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.COLD"
msgid "It's shivering!"
msgstr "Está tremendo!"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.GENERIC"
msgid "I think it's hatching!"
msgstr "Acho que está chocando!"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.HOT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.HOT"
msgid "It'll boil if it doesn't cool down soon."
msgstr "Vai ferver se não esfriar logo."

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.LONG
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.LONG"
msgid "Looks like an egg."
msgstr "Parece um ovo."

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.SHORT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDEGG_CRACKED.SHORT"
msgid "Did that egg just move?"
msgstr "O ovo se mexeu?"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDNEST.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDNEST.GENERIC"
msgid "This could feed us for days!"
msgstr "Isso poderia nos alimentar por dias!"

#. CHARACTERS.WEBBER.DESCRIBE.TALLBIRDNEST.PICKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TALLBIRDNEST.PICKED"
msgid "Where is the egg?"
msgstr "Onde está o ovo?"

#. CHARACTERS.WEBBER.DESCRIBE.TAMALES
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TAMALES"
msgid "Mmm, spicy!"
msgstr "Mmm, picante!"

#. CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.GENERIC"
msgid "It's getting old so fast!"
msgstr "Está envelhecendo muito rápido!"

#. CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.HUNGRY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.HUNGRY"
msgid "It looks hungry!"
msgstr "Parece com fome!"

#. CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.SLEEPING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.SLEEPING"
msgid "Must've had a busy day."
msgstr "Deve ter tido um dia agitado."

#. CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.STARVING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TEENBIRD.STARVING"
msgid "I think it's starving!"
msgstr "Acho que está morrendo de fome!"

#. CHARACTERS.WEBBER.DESCRIBE.TELEBASE.GEMS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEBASE.GEMS"
msgid "It needs something to focus the power."
msgstr "Precisa de algo para concentrar a energia."

#. CHARACTERS.WEBBER.DESCRIBE.TELEBASE.VALID
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEBASE.VALID"
msgid "Seems ready to use."
msgstr "Parece pronto para uso."

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.ACTIVE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.ACTIVE"
msgid "We could use this to visit new worlds!"
msgstr "Poderíamos usar isso para visitar novos mundos!"

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.GENERIC"
msgid "I can hear the sounds of another world!"
msgstr "Posso ouvir os sons de outro mundo!"

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.LOCKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.LOCKED"
msgid "It still won't work!"
msgstr "Ainda não funciona!"

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.PARTIAL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BASE.PARTIAL"
msgid "I don't think we're done yet!"
msgstr "Acho que ainda não terminamos!"

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BOX
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_BOX"
msgid "The power in this box is unimaginable."
msgstr "O poder nessa caixa é inimaginável."

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_CRANK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_CRANK"
msgid "A crank that will stand up to punishment."
msgstr "Uma manivela que suporta punição."

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_POTATO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_POTATO"
msgid "It looks like this goes with something..."
msgstr "Parece que isso encaixa em alguma coisa..."

#. CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_RING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELEPORTATO_RING"
msgid "I think there are more parts."
msgstr "Acho que tem mais peças."

#. CHARACTERS.WEBBER.DESCRIBE.TELESTAFF
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TELESTAFF"
msgid "Just looking at it makes my brain feel fuzzy."
msgstr "Só de olhar faz meu cérebro ficar confuso."

#. CHARACTERS.WEBBER.DESCRIBE.TENT.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENT.BURNT"
msgid "The fire destroyed it."
msgstr "O fogo destruiu tudo."

#. CHARACTERS.WEBBER.DESCRIBE.TENT.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENT.GENERIC"
msgid "It's way past our bedtime!"
msgstr "Passou muito da hora de dormir!"

#. CHARACTERS.WEBBER.DESCRIBE.TENTACLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENTACLE"
msgid "We would be mad if something stepped on us, too."
msgstr "Também ficaríamos brabos se algo pisasse em nós."

#. CHARACTERS.WEBBER.DESCRIBE.TENTACLESPIKE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENTACLESPIKE"
msgid "We could hit stuff with the pointy bits!"
msgstr "Poderíamos golpear coisas com as partes pontudas!"

#. CHARACTERS.WEBBER.DESCRIBE.TENTACLESPOTS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENTACLESPOTS"
msgid "Spotty!"
msgstr "Sardento!"

#. CHARACTERS.WEBBER.DESCRIBE.TENTACLE_GARDEN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENTACLE_GARDEN"
msgid "It's huge!"
msgstr "É enorme!"

#. CHARACTERS.WEBBER.DESCRIBE.TENTACLE_PILLAR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENTACLE_PILLAR"
msgid "It's huge!"
msgstr "É enorme!"

#. CHARACTERS.WEBBER.DESCRIBE.TENTACLE_PILLAR_ARM
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENTACLE_PILLAR_ARM"
msgid "Aw, it's just a baby!"
msgstr "Oun, é só um bebê!"

#. CHARACTERS.WEBBER.DESCRIBE.TENTACLE_PILLAR_HOLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TENTACLE_PILLAR_HOLE"
msgid "Oooh let's jump in!"
msgstr "Uuh, vamos pular lá dentro!"

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.COOLDOWN
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.COOLDOWN"
msgid "Oh no! Did we lose the little tree somewhere?"
msgstr "Ah, não! Perdemos a arvorezinha em algum lugar?"

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.CRIMSON
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.CRIMSON"
msgid "Uh oh... is the tree sick?"
msgstr "Ô-ou... a arvorezinha está doente?"

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.ENABLED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.ENABLED"
msgid "WE DIDN'T SHAKE IT, WE PROMISE!!"
msgstr "NÓS NÃO CHACOALHAMOS ELE, NÓS JURAMOS!!"

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.GENERIC"
msgid "So pretty! We want to shake it!"
msgstr "Tão bonito! Queremos chacoalhar ele!"

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.SPAWN_DISABLED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.SPAWN_DISABLED"
msgid "We think we're done playing with Mr. Eyeball."
msgstr "Acho que já terminamos de brincar com o Sr. Ocular."

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.WAITING_FOR_DARK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUM.WAITING_FOR_DARK"
msgid "We could give it just a little shake, couldn't we?"
msgstr "Poderíamos dar uma chacoalhadinha nele, não?"

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUMCHEST.BURNT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUMCHEST.BURNT"
msgid "Aww, it was such a pretty chest."
msgstr "Ahh, era um baú tão bonito."

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUMCHEST.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUMCHEST.GENERIC"
msgid "We didn't find any toys inside."
msgstr "Não encontramos nenhum brinquedo dentro."

#. CHARACTERS.WEBBER.DESCRIBE.TERRARIUMCHEST.SHIMMER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TERRARIUMCHEST.SHIMMER"
msgid "We bet there are plenty of toys inside!"
msgstr "Apostamos que há muitos brinquedos dentro!"

#. CHARACTERS.WEBBER.DESCRIBE.THULECITE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.THULECITE"
msgid "Fancy rocks!"
msgstr "Pedras bacanas!"

#. CHARACTERS.WEBBER.DESCRIBE.THULECITE_PIECES
msgctxt "CHARACTERS.WEBBER.DESCRIBE.THULECITE_PIECES"
msgid "Small bits of fancy rock."
msgstr "Pedacinhos de uma pedra extravagante."

#. CHARACTERS.WEBBER.DESCRIBE.THURIBLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.THURIBLE"
msgid "It gets really hot."
msgstr "Fica muito quente."

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.ABANDONED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.ABANDONED"
msgid "We'll try our best to find them on our own."
msgstr "Vamos fazer nosso melhor para encontrá-los por conta própria."

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.GENERIC"
msgid "Your majesty!"
msgstr "Sua majestade!"

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.LOST_TRACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.LOST_TRACK"
msgid "Aw, we weren't quick enough."
msgstr "Ah, não fomos rápidos o bastante."

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.NEARBY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.NEARBY"
msgid "Is there a kit nearby, our liege?"
msgstr "Há algum xanim por perto, nosso soberano?"

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.NOTHING_TO_TRACK
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.NOTHING_TO_TRACK"
msgid "It doesn't look like there's anything around here..."
msgstr "Não parece que tem alguma coisa por aqui..."

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.SUCCESS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.SUCCESS"
msgid "Wow! Good work, your highness!"
msgstr "Uau! Ótimo trabalho, vossa alteza!"

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.TARGET_TOO_FAR_AWAY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.TARGET_TOO_FAR_AWAY"
msgid "Maybe the kit's out of his sniffing range?"
msgstr "Talvez o xanim esteja fora do seu alcance de farejamento?"

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.TRACKING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.TRACKING"
msgid "He's onto something!"
msgstr "Ele encontrou alguma coisa!"

#. CHARACTERS.WEBBER.DESCRIBE.TICOON.TRACKING_NOT_MINE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TICOON.TRACKING_NOT_MINE"
msgid "He's helping someone else right now."
msgstr "Ele está ajudando outra pessoa agora."

#. CHARACTERS.WEBBER.DESCRIBE.TILLWEED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TILLWEED"
msgid "It likes to make a mess of our garden."
msgstr "Ela gosta de bagunçar nosso jardim."

#. CHARACTERS.WEBBER.DESCRIBE.TILLWEEDSALVE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TILLWEEDSALVE"
msgid "It helps get rid of booboos."
msgstr "Ajuda a se livrar dos dodóis."

#. CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL.GENERIC"
msgid "Yikes! That's a big frog!"
msgstr "Medo! Esse é um sapo bem grande!"

#. CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL.RAGE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL.RAGE"
msgid "Toadally terrifying!"
msgstr "Vamos encaçapar esse!"

#. CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL_CAP.EMPTY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL_CAP.EMPTY"
msgid "Do you think we'd fit?"
msgstr "Acha que a gente caberia?"

#. CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL_CAP.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL_CAP.GENERIC"
msgid "Mushroom! It's a mushroom!"
msgstr "Cogumelo! É um cogumelo!"

#. CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL_CAP.INGROUND
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOADSTOOL_CAP.INGROUND"
msgid "I think it needs help getting out."
msgstr "Acho que precisa de ajuda pra sair."

#. CHARACTERS.WEBBER.DESCRIBE.TOMATO
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOMATO"
msgid "Is it a fruit or a vegetable?"
msgstr "É uma fruta ou um legume?"

#. CHARACTERS.WEBBER.DESCRIBE.TOMATO_COOKED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOMATO_COOKED"
msgid "It's a cooked fregetable. Vruit?"
msgstr "É uma fregume cozido. Lruta?"

#. CHARACTERS.WEBBER.DESCRIBE.TOMATO_OVERSIZED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOMATO_OVERSIZED"
msgid "Big and juicy."
msgstr "Grande e suculento."

#. CHARACTERS.WEBBER.DESCRIBE.TOMATO_SEEDS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOMATO_SEEDS"
msgid "We can find out what they are by planting them."
msgstr "Podemos descobrir o que são plantando elas."

#. CHARACTERS.WEBBER.DESCRIBE.TOPHAT
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOPHAT"
msgid "Like father used to wear."
msgstr "Igual ao que o pai usava."

#. CHARACTERS.WEBBER.DESCRIBE.TORCH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TORCH"
msgid "This should keep us safe."
msgstr "Isso nos protegerá."

#. CHARACTERS.WEBBER.DESCRIBE.TOURTIERE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOURTIERE"
msgid "We've never had meat in a pie before."
msgstr "A gente nunca tinha comido carne numa torta antes."

#. CHARACTERS.WEBBER.DESCRIBE.TOWNPORTAL.ACTIVE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOWNPORTAL.ACTIVE"
msgid "I hope no one gets a headache."
msgstr "Espero que ninguém fique com dor de cabeça."

#. CHARACTERS.WEBBER.DESCRIBE.TOWNPORTAL.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOWNPORTAL.GENERIC"
msgid "Floaty and weird."
msgstr "É flutuante e estranho."

#. CHARACTERS.WEBBER.DESCRIBE.TOWNPORTALTALISMAN.ACTIVE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOWNPORTALTALISMAN.ACTIVE"
msgid "I think it's ready!"
msgstr "Acho que está pronto."

#. CHARACTERS.WEBBER.DESCRIBE.TOWNPORTALTALISMAN.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TOWNPORTALTALISMAN.GENERIC"
msgid "We didn't know rocks could smell."
msgstr "A gente não sabia que pedras sentiam cheiro."

#. CHARACTERS.WEBBER.DESCRIBE.TRAILMIX
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAILMIX"
msgid "Crunchy and healthy."
msgstr "Crocante e saudável."

#. CHARACTERS.WEBBER.DESCRIBE.TRAILS
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAILS"
msgid "No monkey business, mister!"
msgstr "Sem macaquice, amigo!"

#. CHARACTERS.WEBBER.DESCRIBE.TRANSISTOR
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRANSISTOR"
msgid "A very well crafted doodad!"
msgstr "Um treco muito bem feito!"

#. CHARACTERS.WEBBER.DESCRIBE.TRAP
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAP"
msgid "Now we can catch some food!"
msgstr "Agora podemos capturar comida!"

#. CHARACTERS.WEBBER.DESCRIBE.TRAP_BRAMBLE
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAP_BRAMBLE"
msgid "It's a very pointy planty trap!"
msgstr "É uma armadilha vegetal muito pontuda!"

#. CHARACTERS.WEBBER.DESCRIBE.TRAP_STARFISH.CLOSED
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAP_STARFISH.CLOSED"
msgid "Yikes, that thing was hungry!"
msgstr "Nossa, essa coisa tava faminta!"

#. CHARACTERS.WEBBER.DESCRIBE.TRAP_STARFISH.GENERIC
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAP_STARFISH.GENERIC"
msgid "What a cute starfish!"
msgstr "Que estrela-do-mar bonitinha!"

#. CHARACTERS.WEBBER.DESCRIBE.TRAP_TEETH
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAP_TEETH"
msgid "This will hurt our enemies."
msgstr "Isso machucará nossos inimigos."

#. CHARACTERS.WEBBER.DESCRIBE.TRAP_TEETH_MAXWELL
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TRAP_TEETH_MAXWELL"
msgid "Who would put this here? We could get hurt!"
