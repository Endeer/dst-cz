msgstr "Parece bacalhau."

#. CHARACTERS.WOODIE.DESCRIBE.POND_ALGAE
msgctxt "CHARACTERS.WOODIE.DESCRIBE.POND_ALGAE"
msgid "Doesn't look edible."
msgstr "Não parece comestível."

#. CHARACTERS.WOODIE.DESCRIBE.POOP
msgctxt "CHARACTERS.WOODIE.DESCRIBE.POOP"
msgid "Don't poop in camp!"
msgstr "Sem porcaria no acampamento!"

#. CHARACTERS.WOODIE.DESCRIBE.PORTABLEBLENDER_ITEM
msgctxt "CHARACTERS.WOODIE.DESCRIBE.PORTABLEBLENDER_ITEM"
msgid "Chops up food pretty good."
msgstr "Corta a comida muito bem."

#. CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.COOKING_LONG
msgctxt "CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.COOKING_LONG"
msgid "Better settle in, this could take a while."
msgstr "Melhor me aconchegar, isso pode demorar um pouco."

#. CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.COOKING_SHORT
msgctxt "CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.COOKING_SHORT"
msgid "One meal comin' right up!"
msgstr "A refeição está quase lá!"

#. CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.DONE
msgctxt "CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.DONE"
msgid "Well, should we eat?"
msgstr "Bem, vamos comer?"

#. CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.EMPTY
msgctxt "CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.EMPTY"
msgid "Nothing sadder than an empty crockpot."
msgstr "Nada mais triste do que uma panela vazia."

#. CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.GENERIC
msgctxt "CHARACTERS.WOODIE.DESCRIBE.PORTABLECOOKPOT_ITEM.GENERIC"
msgid "Sure makes the camp smell nice."
msgstr "Deixa um cheiro bom no acampamento."

#. CHARACTERS.WOODIE.DESCRIBE.PORTABLESPICER_ITEM.DONE
msgctxt "CHARACTERS.WOODIE.DESCRIBE.PORTABLESPICER_ITEM.DONE"
