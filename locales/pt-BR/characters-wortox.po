msgid ""
msgstr ""
"Application: Dont' Starve\n"
"POT Version: 2.0\n"

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.CARNIVAL_HOST_HERE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.CARNIVAL_HOST_HERE"
msgid "Now where is our feathered friend?"
msgstr "Agora, onde está seu amigo emplumado?"

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.EMPTY_CATCOONDEN
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.EMPTY_CATCOONDEN"
msgid "No one's home, I'm all alone!"
msgstr "Não há ninguém em casa, estou completamente sozinho!"

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.HOSTBUSY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.HOSTBUSY"
msgid "Goodfeather, hm? I think I know him by another name."
msgstr "Goodfeather, hein? Acho que o conheço por outro nome."

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.KITCOON_HIDEANDSEEK_NOT_ENOUGH_HIDERS
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.KITCOON_HIDEANDSEEK_NOT_ENOUGH_HIDERS"
msgid "We'll need more friends if we want to play, hyuyu!"
msgstr "Vamos precisar de mais amigos se quisermos brincar, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.KITCOON_HIDEANDSEEK_NOT_ENOUGH_HIDING_SPOTS
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.KITCOON_HIDEANDSEEK_NOT_ENOUGH_HIDING_SPOTS"
msgid "The game was denied, there's nowwhere to hide!"
msgstr "A brincadeira não vai render, não há onde se esconder!"

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.KITCOON_HIDEANDSEEK_ONE_GAME_PER_DAY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.KITCOON_HIDEANDSEEK_ONE_GAME_PER_DAY"
msgid "If we play any more, it's gonna be a chore."
msgstr "Pra não ficar massacrante, eles precisam de uns instantes."

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.LOCKED_GATE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.LOCKED_GATE"
msgid "Am I locked out, or in?"
msgstr "Estou preso do lado de fora ou do lado de dentro?"

#. CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.NOCARNIVAL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ACTIVATE.NOCARNIVAL"
msgid "They've come and gone, were they but a dream all along?"
msgstr "Eles vieram e se foram voando, será que eu estava sonhando?"

#. CHARACTERS.WORTOX.ACTIONFAIL.APPLYMODULE.COOLDOWN
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.APPLYMODULE.COOLDOWN"
msgid "only_used_by_wx78"
msgstr "only_used_by_wx78"

#. CHARACTERS.WORTOX.ACTIONFAIL.APPLYMODULE.NOTENOUGHSLOTS
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.APPLYMODULE.NOTENOUGHSLOTS"
msgid "only_used_by_wx78"
msgstr "only_used_by_wx78"

#. CHARACTERS.WORTOX.ACTIONFAIL.APPRAISE.NOTNOW
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.APPRAISE.NOTNOW"
msgid "I tried, but they're occupied."
msgstr "Eu tentei um bocado, mas ele está ocupado."

#. CHARACTERS.WORTOX.ACTIONFAIL.ATTUNE.NOHEALTH
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ATTUNE.NOHEALTH"
msgid "My body would surely die."
msgstr "Meu corpo certamente morreria."

#. CHARACTERS.WORTOX.ACTIONFAIL.BATHBOMB.ALREADY_BOMBED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.BATHBOMB.ALREADY_BOMBED"
msgid "Someone else took the fun!"
msgstr "Outra pessoa estragou a diversão!"

#. CHARACTERS.WORTOX.ACTIONFAIL.BATHBOMB.GLASSED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.BATHBOMB.GLASSED"
msgid "I can't throw it through the looking glass!"
msgstr "Não posso jogá-la através do espelho!"

#. CHARACTERS.WORTOX.ACTIONFAIL.BEGIN_QUEST.ONEGHOST
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.BEGIN_QUEST.ONEGHOST"
msgid "only_used_by_wendy"
msgstr "only_used_by_wendy"

#. CHARACTERS.WORTOX.ACTIONFAIL.BUILD.HASPET
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.BUILD.HASPET"
msgid "I've made my pact with a beast already."
msgstr "Já fiz meu pacto com uma fera."

#. CHARACTERS.WORTOX.ACTIONFAIL.BUILD.MOUNTED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.BUILD.MOUNTED"
msgid "Either I must get down, or the world must come up."
msgstr "Ou eu preciso descer ou o mundo precisa subir."

#. CHARACTERS.WORTOX.ACTIONFAIL.BUILD.TICOON
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.BUILD.TICOON"
msgid "Oh, but one's enough, hyuyu!"
msgstr "Ah, mas um é o bastante, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.CARNIVALGAME_FEED.TOO_LATE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CARNIVALGAME_FEED.TOO_LATE"
msgid "Oh ho, too slow!"
msgstr "Fui muito lento ao dar o alimento!"

#. CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.BURNING
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.BURNING"
msgid "I'm not big on clothes, anyway."
msgstr "Não curto muito roupas mesmo."

#. CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.GENERIC"
msgid "I prefer my current form."
msgstr "Prefiro minha forma atual."

#. CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.INUSE"
msgid "Another soul has use of it right now."
msgstr "Outra alma está usando no momento."

#. CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.NOOCCUPANT
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.NOOCCUPANT"
msgid "I can't begin without a beefalo hitched in."
msgstr "Não dá para começar sem antes um bífalo atrelar."

#. CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.NOTENOUGHHAIR
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CHANGEIN.NOTENOUGHHAIR"
msgid "I could give it some flair... once it has enough hair."
msgstr "Posso lhe dar um visual estonteante... quando a pelagem for abundante."

#. CHARACTERS.WORTOX.ACTIONFAIL.CHARGE_FROM.CHARGE_FULL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CHARGE_FROM.CHARGE_FULL"
msgid "only_used_by_wx78"
msgstr "only_used_by_wx78"

#. CHARACTERS.WORTOX.ACTIONFAIL.CHARGE_FROM.NOT_ENOUGH_CHARGE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CHARGE_FROM.NOT_ENOUGH_CHARGE"
msgid "only_used_by_wx78"
msgstr "only_used_by_wx78"

#. CHARACTERS.WORTOX.ACTIONFAIL.COMPARE_WEIGHABLE.FISH_TOO_SMALL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.COMPARE_WEIGHABLE.FISH_TOO_SMALL"
msgid "This minnow won't win, no!"
msgstr "Esse vairão não vai vencer, não!"

#. CHARACTERS.WORTOX.ACTIONFAIL.COMPARE_WEIGHABLE.OVERSIZEDVEGGIES_TOO_SMALL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.COMPARE_WEIGHABLE.OVERSIZEDVEGGIES_TOO_SMALL"
msgid "I'll have to produce some bigger produce, hyuyuyu!"
msgstr "Vou ter que produzir maiores produtos, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.EMPTY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.EMPTY"
msgid "I can't create from nothing."
msgstr "Não posso criar do nada."

#. CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.INUSE"
msgid "Another soul has use of it right now."
msgstr "Outra alma está usando no momento."

#. CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.MISMATCH
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.MISMATCH"
msgid "I've made an error."
msgstr "Cometi um erro."

#. CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.NOTALLOWED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.CONSTRUCT.NOTALLOWED"
msgid "Oh I simply couldn't."
msgstr "Ah, eu não poderia."

#. CHARACTERS.WORTOX.ACTIONFAIL.COOK.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.COOK.GENERIC"
msgid "I'd rather snack on a soul no one's using."
msgstr "Prefiro lanchar uma alma que ninguém esteja usando."

#. CHARACTERS.WORTOX.ACTIONFAIL.COOK.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.COOK.INUSE"
msgid "Mortals need physical food more than I do."
msgstr "Os mortais precisam de comida física mais do que eu."

#. CHARACTERS.WORTOX.ACTIONFAIL.COOK.TOOFAR
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.COOK.TOOFAR"
msgid "I could hop over, I suppose."
msgstr "Poderia saltar por cima, eu suponho."

#. CHARACTERS.WORTOX.ACTIONFAIL.DISMANTLE.COOKING
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.DISMANTLE.COOKING"
msgid "I'm afraid the pot is far too hot!"
msgstr "Preciso ter em mente que a panela está muito quente!"

#. CHARACTERS.WORTOX.ACTIONFAIL.DISMANTLE.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.DISMANTLE.INUSE"
msgid "Another soul has use of it right now."
msgstr "Outra alma está usando no momento."

#. CHARACTERS.WORTOX.ACTIONFAIL.DISMANTLE.NOTEMPTY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.DISMANTLE.NOTEMPTY"
msgid "First it must be relieved of its possessions."
msgstr "Primeiro, é preciso destituí-la de seus pertences."

#. CHARACTERS.WORTOX.ACTIONFAIL.DRAW.NOIMAGE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.DRAW.NOIMAGE"
msgid "I have no imagination."
msgstr "Não tenho imaginação."

#. CHARACTERS.WORTOX.ACTIONFAIL.FILL_OCEAN.UNSUITABLE_FOR_PLANTS
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.FILL_OCEAN.UNSUITABLE_FOR_PLANTS"
msgid "I'd better abscond to a freshwater pond."
msgstr "Quem sabe se eu fosse até uma lagoa buscar água doce?"

#. CHARACTERS.WORTOX.ACTIONFAIL.FISH_OCEAN.TOODEEP
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.FISH_OCEAN.TOODEEP"
msgid "Stop, abort! My line's too short!"
msgstr "Parar, abortar! Minha linha é curta demais!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.ABIGAILHEART
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.ABIGAILHEART"
msgid "This soul's already been sucked! Hyuyu!"
msgstr "Essa alma já foi absorvida! Huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.BUSY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.BUSY"
msgid "I will try again at a different point in time."
msgstr "Vou tentar de novo num ponto diferente no tempo."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.CANTSHADOWREVIVE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.CANTSHADOWREVIVE"
msgid "Its soul is prevented from returning here."
msgstr "Sua alma foi impedida de voltar aqui."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.CARNIVALGAME_ALREADY_PLAYING
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.CARNIVALGAME_ALREADY_PLAYING"
msgid "Let them have their fun, then I'll show the mortals how it's done."
msgstr "Deixe-os brincar em paz, depois mostrarei aos mortais como é que se faz."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.CARNIVALGAME_INVALID_ITEM
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.CARNIVALGAME_INVALID_ITEM"
msgid "Oh drat, it doesn't want that."
msgstr "Ah, droga. Ele não quer isso."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.DEAD
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.DEAD"
msgid "Their soul's not in right now."
msgstr "Sua alma não está no momento."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.DUPLICATE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.DUPLICATE"
msgid "We cannot learn what we already know!"
msgstr "Não podemos aprender o que já sabemos!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.FOODFULL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.FOODFULL"
msgid "There's already a dish, and it looks delish!"
msgstr "Já tem um prato e parece delicioso!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.GENERIC"
msgid "No, no, no."
msgstr "Na-na-ni-na-não."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.GHOSTHEART
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.GHOSTHEART"
msgid "There's no body for it to return to."
msgstr "Não há corpo para o qual voltar."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.MUSHROOMFARM_NEEDSLOG
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.MUSHROOMFARM_NEEDSLOG"
msgid "Its tummy rumbles for the soul of a log."
msgstr "Sua barriga ronca desejando a alma de um tronco."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.MUSHROOMFARM_NEEDSSHROOM
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.MUSHROOMFARM_NEEDSSHROOM"
msgid "There is only room... for a 'shroom!"
msgstr "Só pode haver um... cogumelo!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.MUSHROOMFARM_NOMOONALLOWED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.MUSHROOMFARM_NOMOONALLOWED"
msgid "It doesn't want to put down roots here."
msgstr "Não quer criar raízes aqui."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOMOON
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOMOON"
msgid "The light of the moon, I hope it comes soon!"
msgstr "A luz da lua, espero que chegue logo!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTDISH
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTDISH"
msgid "It would be in our best interest to not."
msgstr "Seria melhor pra você não fazer isso."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTGEM
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTGEM"
msgid "It couldn't fit if it wanted to!"
msgstr "Não caberia nem se eu quisesse!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTSCULPTABLE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTSCULPTABLE"
msgid "I do not think so, no no no!"
msgstr "Acho que não, não mesmo!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTSTAFF
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.NOTSTAFF"
msgid "That's quite wrong, yes, yes!"
msgstr "Isso está bem errado, é, é!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.PIGKINGGAME_DANGER
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.PIGKINGGAME_DANGER"
msgid "Our souls are in mortal danger right now!"
msgstr "Nossas almas estão correndo perigo mortal!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.PIGKINGGAME_MESSY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.PIGKINGGAME_MESSY"
msgid "This beautiful chaos inhibits me."
msgstr "Esse lindo caos me inibe."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.PIGKINGGAME_TOOLATE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.PIGKINGGAME_TOOLATE"
msgid "The night is upon us!"
msgstr "A noite chegou!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.SLEEPING
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.SLEEPING"
msgid "Their mind inhabits another plane right now."
msgstr "Sua mente habita outro plano agora."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.SLOTFULL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.SLOTFULL"
msgid "It is full already."
msgstr "Já está cheio."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.SPIDERNOHAT
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.SPIDERNOHAT"
msgid "My pocket's too tight for that hat to sit right."
msgstr "Meu bolso é apertado demais para colocar chapéus nesses animais."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.TERRARIUM_COOLDOWN
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.TERRARIUM_COOLDOWN"
msgid "Now wait one minute, there's nothing in it!"
msgstr "Espera aí um minuto, não tem nada oculto!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.TERRARIUM_REFUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.TERRARIUM_REFUSE"
msgid "What a picky little prism!"
msgstr "Que pequeno prisma exigente!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.WRONGGEM
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.WRONGGEM"
msgid "Its color is wrong, wrong, wrong."
msgstr "Sua cor está errada, bem errada."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE.WRONGSHADOWFORM
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE.WRONGSHADOWFORM"
msgid "It looks downright silly, hyuyu!"
msgstr "Parece totalmente bobo, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.BUSY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.BUSY"
msgid "First I must gain their focus."
msgstr "Primeiro preciso obter seu foco."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.DEAD
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.DEAD"
msgid "Their soul's not in right now."
msgstr "Sua alma não está no momento."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.FULL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.FULL"
msgid "Their brim is bursting!"
msgstr "Está transbordando!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.SLEEPING
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVEALLTOPLAYER.SLEEPING"
msgid "Their mind inhabits another plane right now."
msgstr "Sua mente habita outro plano agora."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.BUSY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.BUSY"
msgid "First I must gain their focus."
msgstr "Primeiro preciso obter seu foco."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.DEAD
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.DEAD"
msgid "Their soul's not in right now."
msgstr "Sua alma não está no momento."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.FULL
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.FULL"
msgid "Their brim is bursting!"
msgstr "Está transbordando!"

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.SLEEPING
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVETOPLAYER.SLEEPING"
msgid "Their mind inhabits another plane right now."
msgstr "Sua mente habita outro plano agora."

#. CHARACTERS.WORTOX.ACTIONFAIL.GIVE_TACKLESKETCH.DUPLICATE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.GIVE_TACKLESKETCH.DUPLICATE"
msgid "We cannot learn what we already know!"
msgstr "Não podemos aprender o que já sabemos!"

#. CHARACTERS.WORTOX.ACTIONFAIL.HARVEST.DOER_ISNT_MODULE_OWNER
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.HARVEST.DOER_ISNT_MODULE_OWNER"
msgid "The tin gnat is not keen to chat."
msgstr "O pivete de lata pra falar trava."

#. CHARACTERS.WORTOX.ACTIONFAIL.HERD_FOLLOWERS.WEBBERONLY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.HERD_FOLLOWERS.WEBBERONLY"
msgid "Fiddle dee dee, they won't listen to me!"
msgstr "Upa lelê, não querem me obedecer!"

#. CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.BEEF_HITCHED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.BEEF_HITCHED"
msgid "Surely secured!"
msgstr "Está bem preso!"

#. CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.INMOOD
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.INMOOD"
msgid "My furry friend is too caught up in their furious frenzy."
msgstr "Meu amigo peludo foi tomado pela cólera."

#. CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.NEEDBEEF
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.NEEDBEEF"
msgid "First I must find a beefalo to be mine."
msgstr "Primeiro preciso encontrar um bífalo para me adonar."

#. CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.NEEDBEEF_CLOSER
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.HITCHUP.NEEDBEEF_CLOSER"
msgid "Now now my friend, I can't hitch you up while you're way over there!"
msgstr "Ora, ora, meu amigo. Não posso atrelá-lo se não chegar mais perto de mim!"

#. CHARACTERS.WORTOX.ACTIONFAIL.LOWER_SAIL_FAIL.1
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.LOWER_SAIL_FAIL.1"
msgid "What a fiendishly frustrating contraption! I'm impressed."
msgstr "Que engenhoca diabolicamente frustrante! Estou impressionado."

#. CHARACTERS.WORTOX.ACTIONFAIL.LOWER_SAIL_FAIL.2
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.LOWER_SAIL_FAIL.2"
msgid "Perhaps I should try a new tack! Hyuyu!"
msgstr "Talvez seja melhor tentar uma nova corda! Huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.LOWER_SAIL_FAIL.3
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.LOWER_SAIL_FAIL.3"
msgid "My guise as a sailor was a spectacular failure!"
msgstr "Meu disfarce de marinheiro foi um fracasso espetacular!"

#. CHARACTERS.WORTOX.ACTIONFAIL.MARK.ALREADY_MARKED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.MARK.ALREADY_MARKED"
msgid "Will a change of fate be in order?"
msgstr "Será que teremos uma virada da sorte?"

#. CHARACTERS.WORTOX.ACTIONFAIL.MARK.NOT_PARTICIPANT
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.MARK.NOT_PARTICIPANT"
msgid "Why was I not invited to the festivities?"
msgstr "Por que não fui convidado para as festividades?"

#. CHARACTERS.WORTOX.ACTIONFAIL.MOUNT.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.MOUNT.INUSE"
msgid "It has made a pact with another."
msgstr "Fez um pacto com outro."

#. CHARACTERS.WORTOX.ACTIONFAIL.MOUNT.SLEEPING
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.MOUNT.SLEEPING"
msgid "It's had its forty winks, methinks."
msgstr "Um baita cochilo, fiquei comovido."

#. CHARACTERS.WORTOX.ACTIONFAIL.MOUNT.TARGETINCOMBAT
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.MOUNT.TARGETINCOMBAT"
msgid "Its focus is elsewhere."
msgstr "Seu foco está em outro lugar."

#. CHARACTERS.WORTOX.ACTIONFAIL.OCEAN_FISHING_POND.WRONGGEAR
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.OCEAN_FISHING_POND.WRONGGEAR"
msgid "I fear I've brought the wrong gear, hyuyu!"
msgstr "Receio ter trazido o equipamento errado, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.OPEN_CRAFTING.PROFESSIONALCHEF
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.OPEN_CRAFTING.PROFESSIONALCHEF"
msgid "Warly won't let me peek inside. He's no fun."
msgstr "O Warly não vai me deixar dar uma espiada. Ele não é divertido."

#. CHARACTERS.WORTOX.ACTIONFAIL.OPEN_CRAFTING.SHADOWMAGIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.OPEN_CRAFTING.SHADOWMAGIC"
msgid "I don't think he knows how to use it."
msgstr "Acho que ele não sabe como usar isso."

#. CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.INUSE"
msgid "Another soul has use of it."
msgstr "Outra alma está usando."

#. CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.NOTMINE_YOTC.1
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.NOTMINE_YOTC.1"
msgid "What's the matter? I won't bite."
msgstr "Qual é o problema? Eu não mordo."

#. CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.NOTMINE_YOTC.2
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.NOTMINE_YOTC.2"
msgid "This soul is already bound to someone else."
msgstr "Esta alma já está vinculada à outra pessoa."

#. CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.RESTRICTION
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.PICKUP.RESTRICTION"
msgid "I am cautious about touching it."
msgstr "Preciso ter cautela para tocar."

#. CHARACTERS.WORTOX.ACTIONFAIL.PLANTREGISTRY_RESEARCH_FAIL.FERTILIZER
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.PLANTREGISTRY_RESEARCH_FAIL.FERTILIZER"
msgid "That's all I need to know to make my garden grow."
msgstr "Isso é tudo que quero saber para fazer meu jardim crescer."

#. CHARACTERS.WORTOX.ACTIONFAIL.PLANTREGISTRY_RESEARCH_FAIL.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.PLANTREGISTRY_RESEARCH_FAIL.GENERIC"
msgid "I already know all its tricks, hyuyu!"
msgstr "Já conheço todos os seus truques, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.POUR_WATER.OUT_OF_WATER
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.POUR_WATER.OUT_OF_WATER"
msgid "Oh my, the water's run dry!"
msgstr "Nossa, a água acabou!"

#. CHARACTERS.WORTOX.ACTIONFAIL.POUR_WATER_GROUNDTILE.OUT_OF_WATER
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.POUR_WATER_GROUNDTILE.OUT_OF_WATER"
msgid "Goodness me, it's dry as can be!"
msgstr "Vejam vocês, secou de vez!"

#. CHARACTERS.WORTOX.ACTIONFAIL.REMOVEMODULES.NO_MODULES
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.REMOVEMODULES.NO_MODULES"
msgid "only_used_by_wx78"
msgstr "only_used_by_wx78"

#. CHARACTERS.WORTOX.ACTIONFAIL.REPAIR.WRONGPIECE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.REPAIR.WRONGPIECE"
msgid "That is incorrect!"
msgstr "Isso está incorreto!"

#. CHARACTERS.WORTOX.ACTIONFAIL.REPLATE.MISMATCH
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.REPLATE.MISMATCH"
msgid "Hyuyu! They all look the same to me!"
msgstr "Huehue! Parecem todos iguais pra mim!"

#. CHARACTERS.WORTOX.ACTIONFAIL.REPLATE.SAMEDISH
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.REPLATE.SAMEDISH"
msgid "I cannot redo what's already been done!"
msgstr "Não posso refazer o que já está feito!"

#. CHARACTERS.WORTOX.ACTIONFAIL.ROW_FAIL.BAD_TIMING0
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ROW_FAIL.BAD_TIMING0"
msgid "Time to sync or swim, hyuyu!"
msgstr "É sincronizar ou nadar, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.ROW_FAIL.BAD_TIMING1
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ROW_FAIL.BAD_TIMING1"
msgid "If I time it just right, the end is in sight!"
msgstr "Se eu acertar o timing, logo chegarei ao fim!"

#. CHARACTERS.WORTOX.ACTIONFAIL.ROW_FAIL.BAD_TIMING2
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.ROW_FAIL.BAD_TIMING2"
msgid "I mustn't rock the boat!"
msgstr "Não posso balançar o barco!"

#. CHARACTERS.WORTOX.ACTIONFAIL.RUMMAGE.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.RUMMAGE.GENERIC"
msgid "Oh I simply couldn't."
msgstr "Ah, eu não poderia."

#. CHARACTERS.WORTOX.ACTIONFAIL.RUMMAGE.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.RUMMAGE.INUSE"
msgid "It is making a pact with another."
msgstr "Está fazendo um pacto com outro."

#. CHARACTERS.WORTOX.ACTIONFAIL.RUMMAGE.NOTMASTERCHEF
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.RUMMAGE.NOTMASTERCHEF"
msgid "Warly won't let me peek inside. He's no fun."
msgstr "O Warly não vai me deixar dar uma espiada. Ele não é divertido."

#. CHARACTERS.WORTOX.ACTIONFAIL.SADDLE.TARGETINCOMBAT
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.SADDLE.TARGETINCOMBAT"
msgid "Its focus is elsewhere."
msgstr "Seu foco está em outro lugar."

#. CHARACTERS.WORTOX.ACTIONFAIL.SAIL.REPAIR
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.SAIL.REPAIR"
msgid "The ship is tippy tip top!"
msgstr "O navio está em perfeito estado!"

#. CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.AWAKEBEEFALO
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.AWAKEBEEFALO"
msgid "I may only do that while it sleeps."
msgstr "Só posso fazer isso enquanto dorme."

#. CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.GENERIC"
msgid "I can't, I won't, I shan't."
msgstr "Não posso, não farei, não devo."

#. CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.NOBITS
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.NOBITS"
msgid "Instead of hair, there's nothing there!"
msgstr "Parece que não mais nada de pelos aí!"

#. CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.SOMEONEELSESBEEFALO
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.SHAVE.SOMEONEELSESBEEFALO"
msgid "I think you will find that creature's not mine."
msgstr "Acredite no que quiser, essa criatura minha não é."

#. CHARACTERS.WORTOX.ACTIONFAIL.SLAUGHTER.TOOFAR
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.SLAUGHTER.TOOFAR"
msgid "Oh ho! Too late, they've made their escape."
msgstr "Ah, não! Tarde demais, eles fugiram."

#. CHARACTERS.WORTOX.ACTIONFAIL.START_CARRAT_RACE.NO_RACERS
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.START_CARRAT_RACE.NO_RACERS"
msgid "Now that's quite the trick, invisible racing Carrats!"
msgstr "Que truque fantástico, cenourratos corredores invisíveis!"

#. CHARACTERS.WORTOX.ACTIONFAIL.STORE.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.STORE.GENERIC"
msgid "It's full, full, full."
msgstr "Está cheio, bem cheio."

#. CHARACTERS.WORTOX.ACTIONFAIL.STORE.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.STORE.INUSE"
msgid "Another soul has use of it right now."
msgstr "Outra alma está usando no momento."

#. CHARACTERS.WORTOX.ACTIONFAIL.STORE.NOTALLOWED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.STORE.NOTALLOWED"
msgid "Oh I simply couldn't."
msgstr "Ah, eu não poderia."

#. CHARACTERS.WORTOX.ACTIONFAIL.STORE.NOTMASTERCHEF
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.STORE.NOTMASTERCHEF"
msgid "Warly won't let me peek inside. He's no fun."
msgstr "O Warly não vai me deixar dar uma espiada. Ele não é divertido."

#. CHARACTERS.WORTOX.ACTIONFAIL.TEACH.CANTLEARN
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.TEACH.CANTLEARN"
msgid "My poor imp head cannot contain this knowledge."
msgstr "Minha pobre cabeça de diabrete não pode conter esse conhecimento."

#. CHARACTERS.WORTOX.ACTIONFAIL.TEACH.KNOWN
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.TEACH.KNOWN"
msgid "You cannot learn what you already know."
msgstr "Não se pode aprender o que já se sabe."

#. CHARACTERS.WORTOX.ACTIONFAIL.TEACH.MESSAGEBOTTLEMANAGER_NOT_FOUND
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.TEACH.MESSAGEBOTTLEMANAGER_NOT_FOUND"
msgid "Not here, my dear."
msgstr "Aqui não, meu bem."

#. CHARACTERS.WORTOX.ACTIONFAIL.TEACH.WRONGWORLD
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.TEACH.WRONGWORLD"
msgid "I'm on the wrong plane of existence for this."
msgstr "Estou no plano de existência errado pra isso."

#. CHARACTERS.WORTOX.ACTIONFAIL.TELLSTORY.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.TELLSTORY.GENERIC"
msgid "only_used_by_walter"
msgstr "only_used_by_walter"

#. CHARACTERS.WORTOX.ACTIONFAIL.UNLOCK.WRONGKEY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.UNLOCK.WRONGKEY"
msgid "Either the key is wrong, or the lock is."
msgstr "Ou a chave está errada, ou é a fechadura."

#. CHARACTERS.WORTOX.ACTIONFAIL.USEITEMON.BEEF_BELL_ALREADY_USED
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.USEITEMON.BEEF_BELL_ALREADY_USED"
msgid "Turns out in the end, it already had a friend."
msgstr "Uma certeza eu trago comigo: ele já tinha um amigo."

#. CHARACTERS.WORTOX.ACTIONFAIL.USEITEMON.BEEF_BELL_HAS_BEEF_ALREADY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.USEITEMON.BEEF_BELL_HAS_BEEF_ALREADY"
msgid "Having more than one wouldn't be much fun."
msgstr "Se tivesse mais do que um, ninguém aguentaria o bodum."

#. CHARACTERS.WORTOX.ACTIONFAIL.USEITEMON.BEEF_BELL_INVALID_TARGET
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.USEITEMON.BEEF_BELL_INVALID_TARGET"
msgid "Nope, hyuyuyu!"
msgstr "Negativo, huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.USEKLAUSSACKKEY.KLAUS
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.USEKLAUSSACKKEY.KLAUS"
msgid "Hyuyu!"
msgstr "Huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.USEKLAUSSACKKEY.QUAGMIRE_WRONGKEY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.USEKLAUSSACKKEY.QUAGMIRE_WRONGKEY"
msgid "I believe there is another way."
msgstr "Acredito que haja outro jeito."

#. CHARACTERS.WORTOX.ACTIONFAIL.USEKLAUSSACKKEY.WRONGKEY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.USEKLAUSSACKKEY.WRONGKEY"
msgid "Either the key is wrong, or the lock is."
msgstr "Ou a chave está errada, ou é a fechadura."

#. CHARACTERS.WORTOX.ACTIONFAIL.WRAPBUNDLE.EMPTY
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.WRAPBUNDLE.EMPTY"
msgid "I need something to bind within."
msgstr "Preciso de alguma coisa pra unir."

#. CHARACTERS.WORTOX.ACTIONFAIL.WRITE.GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.WRITE.GENERIC"
msgid "What is written cannot be unwrote!"
msgstr "O que está escrito não pode ser desescrito!"

#. CHARACTERS.WORTOX.ACTIONFAIL.WRITE.INUSE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.WRITE.INUSE"
msgid "Someone else has command of it."
msgstr "Outra pessoa está comandando."

#. CHARACTERS.WORTOX.ACTIONFAIL.YOTB_STARTCONTEST.ALREADYACTIVE
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.YOTB_STARTCONTEST.ALREADYACTIVE"
msgid "Perhaps he too likes to travel between planes? Hyuyuyu!"
msgstr "Talvez ele também goste de viajar entre os planos? Huehue!"

#. CHARACTERS.WORTOX.ACTIONFAIL.YOTB_STARTCONTEST.DOESNTWORK
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.YOTB_STARTCONTEST.DOESNTWORK"
msgid "Oh ho! Something's gone awry!"
msgstr "Oh, ho! Ocorreu algum problema!"

#. CHARACTERS.WORTOX.ACTIONFAIL.YOTB_UNLOCKSKIN.ALREADYKNOWN
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL.YOTB_UNLOCKSKIN.ALREADYKNOWN"
msgid "Oh what a bore, I've seen it before."
msgstr "Ah, que entediante, já tinha visto isso antes."

#. CHARACTERS.WORTOX.ACTIONFAIL_GENERIC
msgctxt "CHARACTERS.WORTOX.ACTIONFAIL_GENERIC"
msgid "No, no, no."
msgstr "Na-na-ni-na-não."

#. CHARACTERS.WORTOX.ANNOUNCE_ACCOMPLISHMENT
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ACCOMPLISHMENT"
msgid "I feel excellent about myself!"
msgstr "Estou me sentindo excelente comigo mesmo!"

#. CHARACTERS.WORTOX.ANNOUNCE_ACCOMPLISHMENT_DONE
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ACCOMPLISHMENT_DONE"
msgid "I've done the thing!"
msgstr "Consegui!"

#. CHARACTERS.WORTOX.ANNOUNCE_ADVENTUREFAIL
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ADVENTUREFAIL"
msgid "I've had enough plane hopping."
msgstr "Chega de saltar entre planos."

#. CHARACTERS.WORTOX.ANNOUNCE_ANTLION_SINKHOLE.1
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ANTLION_SINKHOLE.1"
msgid "Tremors and trembles!"
msgstr "Tremores e estremecimentos!"

#. CHARACTERS.WORTOX.ANNOUNCE_ANTLION_SINKHOLE.2
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ANTLION_SINKHOLE.2"
msgid "Crumbles and cracks!"
msgstr "Desmoronamentos e fendas!"

#. CHARACTERS.WORTOX.ANNOUNCE_ANTLION_SINKHOLE.3
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ANTLION_SINKHOLE.3"
msgid "Seismic shivers, seismic shakes!"
msgstr "Tremores sísmicos, agitações sísmicas!"

#. CHARACTERS.WORTOX.ANNOUNCE_ANTLION_TRIBUTE.1
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ANTLION_TRIBUTE.1"
msgid "Tidings and tithings to you!"
msgstr "Bons dias e bons dízimos pra você!"

#. CHARACTERS.WORTOX.ANNOUNCE_ANTLION_TRIBUTE.2
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ANTLION_TRIBUTE.2"
msgid "Chin up!"
msgstr "Cabeça pra cima!"

#. CHARACTERS.WORTOX.ANNOUNCE_ANTLION_TRIBUTE.3
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ANTLION_TRIBUTE.3"
msgid "Gifts all around! Or for you, at least."
msgstr "Presentes por todos os lados! Ou pra você, pelo menos."

#. CHARACTERS.WORTOX.ANNOUNCE_ARCHIVE_NEW_KNOWLEDGE
msgctxt "CHARACTERS.WORTOX.ANNOUNCE_ARCHIVE_NEW_KNOWLEDGE"
