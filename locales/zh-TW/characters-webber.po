msgstr "重量: {weight}\n捕獲人: {owner}"

#. CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_FISH.HAS_ITEM_HEAVY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_FISH.HAS_ITEM_HEAVY"
msgid "Weight: {weight}\nCaught by: {owner}\nWe've never seen such a heavy fish!"
msgstr "重量: {weight}\n捕獲人: {owner}\n我們就沒見過這麼重的魚！"

#. CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_FISH.OWNER
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_FISH.OWNER"
msgid "Weight: {weight}\nCaught by: {owner}\nWe did it!"
msgstr "重量: {weight}\n捕獲人: {owner}\n我們成功了！"

#. CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_FISH.OWNER_HEAVY
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_FISH.OWNER_HEAVY"
msgid "Weight: {weight}\nCaught by: {owner}\nWe caught it all by ourselves!"
msgstr "重量: {weight}\n捕獲人: {owner}\n我們自己抓到的！"

#. CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_OVERSIZEDVEGGIES.BURNING
msgctxt "CHARACTERS.WEBBER.DESCRIBE.TROPHYSCALE_OVERSIZEDVEGGIES.BURNING"
